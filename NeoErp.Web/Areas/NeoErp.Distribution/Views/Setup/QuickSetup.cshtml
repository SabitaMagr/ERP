        $("#distMapping").click(function () {
            debugger;
            var $this = $(this);
            $this.button('loading');
            $.ajax({
                url: '/api/QuickSetup/DistributorResellerMapping',
                type: "GET",
                success: function (result) {
                    displayPopupNotification(result.MESSAGE, result.TYPE);
                    $this.button('reset');
                },
                error: function (err) {
                    displayPopupNotification(err.statusText, "error");
                    $this.button('reset');
                }
            });
        });

        $("#entityId").click(function () {
            var $this = $(this);
            $this.button('loading');
            $.ajax({
                url: "/api/QuickSetup/CreateEntities",
                type: 'GET',
                success: function (response) {
                    displayPopupNotification(response.MESSAGE, response.TYPE);
                    $this.button('reset');
                },
                error: function (response) {
                    displayPopupNotification(err.statusText, "error");
                    $this.button('reset');
                },
            });
        });

        $("#CreateDefaultValues").click(function () {
            var $this = $(this);
            $this.button('loading');
            $.ajax({
                url: "/api/QuickSetup/CreateDefaultValues",
                type: 'POST',
                success: function (response) {
                    displayPopupNotification(response.MESSAGE, response.TYPE);
                    $this.button('reset');
                },
                error: function (err) {
                    displayPopupNotification(err.statusText, "error");
                    $this.button('reset');
                },
            });
        });

        $("#UpdateResellerCreatedBy").click(function () {
            var $this = $(this);
            $this.button('loading');
            $.ajax({
                url: "/api/QuickSetup/UpdateCreatedByReseller",
                type: 'POST',
                success: function (response) {
                    displayPopupNotification(response.MESSAGE, response.TYPE);
                    $this.button('reset');
                },
                error: function (err) {
                    displayPopupNotification(err.statusText, "error");
                    $this.button('reset');
                },
            });
        });
    });

    $("#RemoveDuplicateRoutes").click(function () {
        var $this = $(this);
        $this.button('loading');
        $.ajax({
            url: "/api/QuickSetup/RemoveDuplicateRoutes",
            type: 'POST',
            success: function (response) {
                displayPopupNotification(response.MESSAGE, response.TYPE);
                $this.button('reset');
            },
            error: function (err) {
                displayPopupNotification(err.statusText, "Error while creating the duplicate routes!");
                $this.button('reset');
            },
        });
    });

