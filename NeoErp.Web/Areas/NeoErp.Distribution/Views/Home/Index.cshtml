<script>
   // initialize();
    $('#toggle-switcher').click(function () {
        if ($(this).hasClass('opened')) {
            $(this).removeClass('opened');
            $(".gride-2").hide();
            $('#style-switcher').animate({ 'left': '-40px', 'width': '37px' });
        } else {
            $(this).addClass('opened');
            $(".gride-2").show();
            $('#style-switcher').animate({ 'left': '-488px', 'width': '500px' });
        }
    });


    //switcher-2





    //close on background click
    $('#style-switcher').click(function (e) { //button click class name is myDiv
        e.stopPropagation();

    });
    $(document).click(function () {
        $("#toggle-switcher").removeClass('opened');
        $(".gride-2").hide();
        $('#style-switcher').animate({ 'left': '-40px', 'width': '37px' });
    });


    var widgetsUrl = '@Url.Content("~/Distribution/Home/DashBoardWidgets")';
    $(document).ready(function () {       
        $.ajax({
            type: 'POST',
            url: widgetsUrl,
            success: function (data) {
                $('#dashboardWidget').html(data);
            }
        });
    })
</script>
