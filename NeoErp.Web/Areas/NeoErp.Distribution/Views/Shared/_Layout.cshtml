    <script>
        window.virtualPath = '@Url.Content("~")';
        jQuery(document).ready(function () {
            Metronic.init(); // init metronic core componets
            Layout.init(); // init layout
            QuickSidebar.init(); // init quick sidebar
            Demo.init(); // init demo features
            Index.init();
            Index.initDashboardDaterange();
            Index.initJQVMAP(); // init index page's custom scripts
            Index.initCalendar(); // init index page's custom scripts
            Index.initCharts(); // init index page's custom scripts
            Index.initChat();
            Index.initMiniCharts();
            Tasks.initDashboardWidget();
            key('ctrl+h', function () { $('.setting').hide(); return false });
            key('ctrl+v', function () { $('.setting').show(); return false });

            $(".themes-select").on("change", function () {
                $('link[title][rel="alternate stylesheet"]').prop('disabled', true);
                $('link[title="' + $(this).val() + '"][rel="alternate stylesheet"]').prop('disabled', false);
                $.cookie("themes", $(this).val(), { expires: 365, path: '/' });

            });

            if ($.cookie("themes") != undefined) {
                $(".themes-select").val($.cookie("themes"));
                $(".themes-select").trigger("change");
            }


            //for notifications
            $.get(window.location.protocol + "//" + window.location.host + "/api/Setup/GetHeaderNotifications", function (data) {
                $("#header_notification_length").html(data.length);
                for (var len = 0; len < data.length; len++) {
                    var elem = "<li>\
                            <a href="+ data[len].Key +">\
                                <span class=\"details\">\
                                    <span class=\"label label-sm label-icon label-success\">\
                                        <i class=\"fa fa-bell-o\"></i>\
                                    </span>"+data[len].Value +"\
                                </span>\
                            </a>\
                        </li>";
                    $("#header_notification_body").append(elem);
                }
                $('#header_notification_body').slimScroll({
                    position: 'right',
                    height: '250px',
                    railVisible: true,
                    alwaysVisible: false,
                });
            }).fail(function () {
                displayPopupNotification("Cannot get notifications", "error");
            });
        });
    </script>
