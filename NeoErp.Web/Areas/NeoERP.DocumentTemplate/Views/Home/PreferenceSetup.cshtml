<div ng-controller="PreferenceSetupCtrl" data-ng-init="init()" style="font-size: 12px !important;">
    <div class="portlet box green-box main">

        <div class="portlet-title">
            <div class="caption caption-title ng-binding">
                <i class="fa fa-list-alt"></i><span class="caption-subject bold uppercase">Preference Setup</span>
            </div>
            <div class="actions">
                <a class="btn dark-green btn-sm addcolor" id="createbtn" ng-click="showModalForNew(event)">
                    <i class="fa fa-plus"></i> NEW
                </a>
            </div>
        </div>
        <div class="portlet-body">
            <kendo-grid id="kGrid" options="companyCenterChildGridOptions" k-rebind="branchCenterChildGridOptions">
            </kendo-grid>
        </div>

