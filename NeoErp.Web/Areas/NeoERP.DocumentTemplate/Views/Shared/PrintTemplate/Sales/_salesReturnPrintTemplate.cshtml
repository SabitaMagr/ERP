                    <thead>
                        <tr ng-repeat="element in MasterFormElement | orderBy:'SERIAL_NO'" ng-if="element.COLUMN_NAME.indexOf('NO') !== -1">
                            <th ng-if="element.COLUMN_NAME.indexOf('INVOICE_NO') !== -1" style="width:100%"><span>Invoice No#</span></th>
                            <th ng-if="element.COLUMN_NAME.indexOf('INVOICE_NO') === -1" style="width:100%"><span>{{element.COLUMN_HEADER}}</span></th>
                            <td>
                                <span>{{masterModels[element.COLUMN_NAME]}}</span>
                            </td>
                        </tr>
                    </thead>
                </table>*@
        </div>
    </header>

    <div class="invoice_title">
        <h2>{{printCompanyInfo.formName}}</h2>
    </div>

    <table class="print_table fnce table-bordered">
        <thead>
            <tr>
                <th style="width:90%;">Description of Goods</th>
                <th>Per</th>
                <th>Quantity</th>
                <th>Rate</th>
                <th>Amount</th>
            </tr>
        </thead>
        <tbody>
            <tr ng-repeat="(key,row) in print_body_col | orderBy: 'SERIAL_NO'" style="height:20px">
                <td ng-repeat="item in row.element | orderBy: 'SERIAL_NO'" ng-hide="item.COLUMN_NAME==='PARTICULARS' || item.COLUMN_NAME==='TRANSACTION_TYPE' || item.COLUMN_NAME==='CALC_TOTAL_PRICE' || item.COLUMN_NAME==='CALC_UNIT_PRICE'|| item.COLUMN_NAME==='COMPLETED_QUANTITY'|| item.COLUMN_NAME==='CALC_QUANTITY' || item.COLUMN_NAME==='FROM_LOCATION_CODE'">
                    <div ng-if="item.COLUMN_NAME.indexOf('CODE') !== -1">
                        <div ng-if="item.COLUMN_NAME==='ITEM_CODE'">
                            <span>{{childModels[key][item.COLUMN_NAME]}}</span>

                        </div>
                        <div ng-if="item.COLUMN_NAME==='PRODUCT_CODE'">
                            <span>{{childModels[key][item.COLUMN_NAME]}}</span>

                        </div>
                    </div>

                    <div ng-if="item.COLUMN_NAME==='QUANTITY'" style="text-align:right;">
                        <span>{{childModels[key][item.COLUMN_NAME]}}</span>
                    </div>
                    <div ng-if="item.COLUMN_NAME==='UNIT_PRICE'" style="text-align:right;">
                        <span>{{childModels[key][item.COLUMN_NAME]}}</span>
                    </div>
                    <div ng-if="item.COLUMN_NAME.indexOf('CODE') !== -1">
                        <div ng-if="item.COLUMN_NAME==='MU_CODE'">
                            <span>{{childModels[key][item.COLUMN_NAME]}}</span>

                        </div>

                    </div>
                    <div ng-if="item.COLUMN_NAME==='TOTAL_PRICE'" style="text-align:right;">
                        <span>{{childModels[key][item.COLUMN_NAME]}}</span>
                    </div>
                </td>
            </tr>
        </tbody>
    </table>

    <table class="print_table balance table-bordered">
        <tr>
            <td style="width:993px; text-align:right">{{totalQty}}</td>
            <td colspan="2" style="width:99px;text-align:right">Net Total</td>
            <td style="width:99px;text-align:right">{{summary.grandTotal}}</td>

        </tr>
        <tr>
            <th style="width:993px;"><span>Amount in words :</span> {{amountinword}} Only.</th>
        </tr>
    </table>

    <p> Through: Cash In Hand -Factory</p>

    <div class="invoice_footer">
        <div class="narration">
            <table class="print_table meta table-bordered1">
                <tbody>
                    <tr ng-repeat="element in MasterFormElement | orderBy:'SERIAL_NO'">
                        `
                        <td ng-if="element.COLUMN_NAME.indexOf('REMARKS') !== -1">
                            <p><strong>Narration :</strong>{{masterModels[element.COLUMN_NAME]}}</p>
                        </td>
                    </tr>
                </tbody>
            </table>
        </div>

        <div class="invoice_footer_details">
            <ul>
                <li>
                    <div class="by">Rajesh</div>
                    Prepared By:
                </li>
                <li>
                    <div class="by">Hira</div>
                    Receiver Signature :
                </li>
                <li>
                    <div class="by">Dinesh</div>
                    Checked By :
                </li>
                <li>
                    <div class="by">Rajesh</div>
                    Authorised Signatory :
                </li>
            </ul>
        </div>

    </div>
</div>