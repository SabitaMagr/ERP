

<link href="@Styles.Url("~/Areas/NeoERP.DocumentTemplate/Content/inventory.css")" rel="stylesheet" media="print" />
<link href="@Styles.Url("~/Areas/NeoERP.DocumentTemplate/Content/inventory.css")" rel="stylesheet" />

<div class="printable-modal">
    <header class="p_header">
        <div> {{printCompanyInfo.form_edesc}}</div>
        <div class="companylogo">
            <img alt="" src="~/Images/logo.png">
        </div>
        <div class="info">
            <h4> {{printCompanyInfo.companyName}}<br>{{printCompanyInfo.form_edesc}}</h4>
            <p>{{printCompanyInfo.address}}</p>
            <p>Ph.No. : {{printComnayInfo.TELEPHONE}}</p>
            <p>Mobile : 9854035126,9854035122</p>
            <p>E-Mail : {{printCompanyInfo.email}}</p>
            <p>TPIN No.: {{printCompanyInfo.tPinVatNo}}</p>


        </div>
        <div>
            <table class="print_table consigneetable table table-bordered">
                <thead>

                </thead>
                <tbody>
                    <tr>
                        <td>
                            <span>Consignee: {{masterModels.CUSTOMER_CODE}}</span>
                            <span>Address: {{masterModels.REGD_OFFICE_EADDRESS}}</span>
                            <span>Party Type: {{masterModels.TEL_MOBILE_NO1}}</span>
                            <span>Pan No:{{masterModels.TPIN_VAT_NO}}</span>
                        </td>
                    </tr>
                </tbody>

            </table>
        </div>
        <div class="date">
            <table class="print_table meta">
                <tr>
                    <td>
                        <div ng-repeat="element in MasterFormElement | orderBy: 'SERIAL_NO'" ng-if="element.COLUMN_NAME.indexOf('REMARKS') == -1">
                            <div ng-if="element.COLUMN_NAME.indexOf('NO') !== -1">
                                <div ng-if="element.COLUMN_NAME.indexOf('CHALAN_NO') !== -1">
                                    <label for="element">{{element.COLUMN_HEADER}}</label>
                                    <div><span>{{masterModels[element.COLUMN_NAME]}}</span></div>
                                </div>
                                <div ng-if="element.COLUMN_NAME.indexOf('ORDER_NO') !== -1">
                                    <label for="element">{{element.COLUMN_HEADER}}</label>
                                    <div><span>{{masterModels[element.COLUMN_NAME]}}</span></div>
                                </div>
                                <div ng-if="element.COLUMN_NAME.indexOf('RETURN_NO') !== -1">
                                    <label for="element">{{element.COLUMN_HEADER}}</label>
                                    <div><span>{{masterModels[element.COLUMN_NAME]}}</span></div>
                                </div>
                                <div ng-if="element.COLUMN_NAME.indexOf('SALES_NO') !== -1">
                                    <label for="element">{{element.COLUMN_HEADER}}</label>
                                    <div><span>{{masterModels[element.COLUMN_NAME]}}</span></div>
                                </div>

                            </div>
                        </div>
                    </td>
                    <td>
                        <div ng-repeat="element in MasterFormElement | orderBy: 'SERIAL_NO'" ng-if="element.COLUMN_HEADER.indexOf('Date') != -1">

                            <label for="element">{{element.COLUMN_HEADER}}</label>
                            <div>
                                <span>{{masterModels[element.COLUMN_NAME]}}</span>
                                <span>[{{getNepaliDate(masterModels[element.COLUMN_NAME])}}]</span>
                            </div>

                        </div>
                    </td>
                </tr>

                <tr>
                    <td>
                        <div ng-repeat="element in MasterFormElement | orderBy: 'SERIAL_NO'" ng-if="element.COLUMN_NAME.indexOf('REMARKS') == -1">
                            <div ng-if="element.COLUMN_NAME.indexOf('CODE') !== -1">
                                <div ng-if="element.COLUMN_NAME.indexOf('FROM_LOCATION_CODE') !== -1">
                                    <label for="element">{{element.COLUMN_HEADER}}</label>
                                    <div><span>{{masterModels[element.COLUMN_NAME]}}</span></div>
                                </div>

                            </div>
                        </div>
                    </td>
                    <td>
                        <div ng-repeat="element in MasterFormElement | orderBy: 'SERIAL_NO'" ng-if="element.COLUMN_NAME.indexOf('REMARKS') == -1">
                            <div ng-if="element.COLUMN_NAME.indexOf('CODE') !== -1">
                                <div ng-if="element.COLUMN_NAME.indexOf('TO_LOCATION_CODE') !== -1">
                                    <label for="element">{{element.COLUMN_HEADER}}</label>
                                    <div><span>{{masterModels[element.COLUMN_NAME]}}</span></div>
                                </div>
                            </div>
                        </div>
                    </td>


                </tr>



                <tr>
                    <td>
                        <div ng-repeat="(key, value) in customModels">
                            <div ng-if="key==='DESPATCH THROUGH'">
                                <label for="element">{{key}}</label>
                                <div><span>{{value}}</span></div>
                            </div>
                        </div>
                    </td>
                    <td>
                        <div ng-repeat="(key, value) in customModels">
                            <div ng-if="key==='TERMS OF PAYMENT'">
                                <label for="element">{{key}}</label>
                                <div><span>{{value}}</span></div>
                            </div>
                        </div>
                    </td>

                </tr>
                <tr>
                    <td>
                        <div ng-repeat="(key, value) in customModels">
                            <div ng-if="key==='TRUCK NO.'">
                                <label for="element">{{key}}</label>
                                <div><span>{{value}}</span></div>
                            </div>
                        </div>

                    </td>
                    <td>
                        <div ng-repeat="(key, value) in customModels">
                            <div ng-if="key==='DESTINATION'">
                                <label for="element">{{key}}</label>
                                <div><span>{{value}}</span></div>
                            </div>
                        </div>
                    </td>
                </tr>
                <tr>
                    <td>
                        <div ng-repeat="(key, value) in customModels">
                            <div ng-if="key==='BUYERS ORDER NO.'">
                                <label for="element">{{key}}</label>
                                <div><span>{{value}}</span></div>
                            </div>
                        </div>
                    </td>
                    <td>
                        <div ng-repeat="(key, value) in customModels">
                            <div ng-if="key==='DATED'">
                                <label for="element">{{key}}</label>
                                <div><span>{{value}}</span></div>
                            </div>
                        </div>
                    </td>
                </tr>
                <tr>
                    <td>
                        <div ng-repeat="(key, value) in customModels">
                            <div ng-if="key==='DISPATCH DOCUMENT NO.'">
                                <label for="element">{{key}}</label>
                                <div><span>{{value}}</span></div>
                            </div>
                        </div>
                    </td>
                    <td>
                        <div ng-repeat="(key, value) in customModels">
                            <div ng-if="key==='DATED'">
                                <label for="element">{{key}}</label>
                                <div><span>{{value}}</span></div>
                            </div>
                        </div>
                    </td>
                </tr>
                <tr>
                    <td colspan="2">
                        <div ng-repeat="(key, value) in customModels">
                            <div ng-if="key==='TERMS OF DELIVERY'">
                                <label for="element">{{key}}</label>
                                <div><span>{{value}}</span></div>
                            </div>
                        </div>
                    </td>
                </tr>



            </table>

