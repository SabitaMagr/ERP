<input id="item.COLUMN_NAME"
       k-data-text-field="'SUB_CODE'"
       k-data-value-field="'SUB_CODE'"
       ng-blur="Changesubledgercode(key,$index,kendoEvent)"
       k-filter="'contains'"
       kendo-combo-box
       subledger-key="{{key}}"
       subledger-index="{{$index}}"
       ng-model="dynamicSubLedgerModalData[key].SUBLEDGER[$index].SUB_CODE"
       k-auto-bind="true"
       k-scrollable="true"
       class="subledgerAutoComplete subledgerfirst SUBLEDGERCODE_{{$index}}"
       ng-keyup="popUp(key)"
       k-options="subledgerCodeAutocomplete" next-on-tab />