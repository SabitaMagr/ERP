<div ng-controller="paymentmodeCtrl" id="paymentmodeCtrl">
    
        <label for="element">{{element.COLUMN_HEADER}} <i class="fa fa-info-circle font-green" data-toggle="tooltip" title="{{element.HELP_DESCRIPTION}}"></i><span style="color:red">{{paymentmodeerror}}</span></label>
   
    <input id="currency" ng-model="masterModels[element.COLUMN_NAME]" name="paymentmode" class="paymentmode mpaymentmode c w-100" kendo-drop-down-list
           k-placeholder="'Select Payment Mode'"
           k-data-text-field="'PAYMENT_MODE_EDESC'"
           k-data-value-field="'PAYMENT_MODE_CODE'"
           k-option-label="'Select Payment Mode'"
           k-filter="'contains'"
           k-suggest="true"
           k-data-source="paymentmodeDataSource" />
</div>



