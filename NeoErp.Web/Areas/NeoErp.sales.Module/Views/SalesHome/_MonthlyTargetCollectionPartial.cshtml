<div class="portlet portlet-sortable  light bordered" id="MonthlyTargetCollectionPartial">
    <div class="portlet-title">
        <div class="caption">
            <i class="icon-bar-chart font-green-haze"></i>
            <span class="caption-subject bold uppercase font-green-haze custom-modal">Monthly Target vs Collection</span>
        </div>
        <div class="tools">
            <a href="javascript:;" class="collapse"></a>
            <a class="btnGrid btnDataGrid_MonthlyTargetCollectionPartial" data-type="Chart" data-title="Grid" data-catagoryAxis="Month">
                <i class="fa fa-table"></i>
            </a>
            <div class="btn-group">
                <a class="btn dropdown-toggle download-btn" title="Download" data-toggle="dropdown" href="#">
                    <i class="icon-arrow-down Firstcustom"></i>

                </a>
                <ul class="dropdown-menu custom">
                    <div class="btn-group-custom">
                        <button type="button" data-id="TargetCollectionChart" class="export-pdf btn default blue-stripe"><i class="fa fa-file-pdf-o"></i> PDF</button>
                        <button type="button" data-id="TargetCollectionChart" class="export-img btn default red-stripe"><i class="fa fa-image-o"></i>Image</button>
