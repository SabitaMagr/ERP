    <div class="portlet-body">
        <script type="text/x-kendo-template" id="toolbar-template">
            <div class="k-header k-grid-toolbar pull-right">

                <a class="k-button k-button-icontext k-grid-excel" href="javascript:">
                    <i class="fa fa-file-excel-o"></i>
                </a>
        </script>
        <div class="k-content wide">
            <div id="TargetCollectionChart"></div>
            <div class="AmountFigureType"></div>
            <div class="DisplayFilterContent"><a href="javascript:;" data-toggle="tooltip" title=""></a></div>
        </div>
    </div>

    <script>
        var dataProvider = [];
        var series_MonthlyTargetCollectionPartial = [{
            field: "Target",
            name: "Target"
        }, {
            field: "Collection",
            name: "Collection"
        }];
        var IsStack = true;
        var colors = ["#428bca", "#da3b36", "#e67d4a", "#5bc0de", "#5cb85c", "#f2b661"];
        var toggleFlag_MonthlyTargetCollectionPartial = true;

        $('#TargetCollectionChart').closest('.portlet').find('.reload').click(function () {
            $('.btnDataGrid_MonthlyTargetCollectionPartial').data("catagoryAxis", "Month");
            createMonthlyTargetCollectionChart();
            RefreshChartFilterControl('@actionPageId');
        });

        $('#TargetCollectionChart').closest('.portlet').find('.fullscreen').click(function () {
            if ($(this).hasClass("on")) {
                $("#TargetCollectionChart").data("kendoChart").options.chartArea.height = 350;
            }
            else {
                $("#TargetCollectionChart").data("kendoChart").options.chartArea.height = 550;
            }
            $("#TargetCollectionChart").data("kendoChart").dataSource.read();
            $("#TargetCollectionChart").data("kendoChart").refresh();
        });
        $('#TargetCollectionChart').closest('.portlet').find('.config').click(function () {
            $(this).closest('.portlet').css('zIndex', 10040);
        });

        $('#TargetCollectionChart').closest('.portlet').find('.icon-arrow-down').click(function () {
            $(this).closest('.portlet').css('zIndex', 10040);
        });


        function createMonthlyTargetCollectionChart(urlData) {
            urlData = (urlData == undefined) ? "?customerCode=&itemCode=&categoryCode=&companyCode=&branchCode=" + branchCodes + "&partyTypeCode=&formCode=" : urlData;
            var dateformat = $("input[name=DateFormat_TargetCollection]:checked").val();
            var catagoryAxisName = "MonthYear", sortaxix = "Month";
            $('.btnDataGrid_MonthlyTargetCollectionPartial').data("catagoryAxis", "Month");
            var mainUrl = window.location.protocol + "//" + window.location.host + "/api/SalesDashboard/GetTargetCollectionMonthlyReport";
            var daywiseUrl = window.location.protocol + "//" + window.location.host + "/api/SalesDashboard/GettargetCollectionDailyReport";
            var IsDataGrid = $('.btnDataGrid_MonthlyTargetCollectionPartial').data("type");
             reportFilter = reportFilter == undefined ? ReportFilter.filterAdditionalData() : reportFilter;
            var dataSource = new kendo.data.DataSource({
                transport: {
                    read: {
                        url: mainUrl + "?DateFormat=" + dateformat,
                        dataType: "json", // <-- The default was "jsonp".
                        type: "POST",
                        contentType: "application/json; charset=utf-8"
                    },
                    parameterMap: function (options, type) {
                        var paramMap = JSON.stringify($.extend(options, reportFilter));
                        delete paramMap.$inlinecount; // <-- remove inlinecount parameter.
                        delete paramMap.$format; // <-- remove format parameter.
                        return paramMap;
                    }
                },
                requestStart: function () {
                    if (IsDataGrid != "Grid") {
                        kendo.ui.progress($("#TargetCollectionChart"), true);
                    }
                },
                requestEnd: function () {
                    kendo.ui.progress($("#TargetCollectionChart"), false);
                },
                //sort: {
                //    field: sortaxix,
                //    dir: "asc"
                //},
                schema: {
                    parse: function (items) {

                        var item,
                            result = [];
                        var showzerovalue = $("#showzerovalue_monthlyTargetCollection").prop("checked");

                        if (showzerovalue) {
                            for (var i = 0; i < items.length; i++) {
                                item = items[i];
                                result.push(item);

                            }

                        }
                        else {
                            for (var i = 0; i < items.length; i++) {
                                item = items[i];
                                if (item.Collection !== 0 && item.target !== 0) {
                                    result.push(item);
                                }
                            }
                        }
                        return result;
                    }
                }
            });
            dataProvider = dataSource;
            $("#MonthlyTargetCollectionPartial .DisplayFilterContent a").text("");
            $(this).parent($("#TargetCollectionChart")).html('')
            $("#TargetCollectionChart").html('');
            // data-grid set option on ceate funciton
            if (IsDataGrid == "Grid") {
                LoadDataGrid_MonthlyTargetCollectionPartial(dataSource);
                return;
            }

            LoadMonthlyTargetCollectionChart(dataSource);

        }
        function LoadMonthlyTargetCollectionChart(dataSource) {
            var daywiseUrl = window.location.protocol + "//" + window.location.host + "/api/SalesDashboard/GettargetCollectionDailyReport";
            $("#TargetCollectionChart").kendoChart({
                chartArea: {
                    height: $("#MonthlyTargetCollectionPartial").hasClass("portlet-fullscreen") ? 550 : 350
                },
                legend: {
                    position: "bottom"
                },
                seriesDefaults: {
                    type: $("input[name=seriesType_TargetCollection]:checked").val(),
                    style: "smooth",
                    stack: $("#targetCollectionInStack").prop("checked"),
                    labels: {
                        visible: $("#showlables_monthlyTargetCollection").prop("checked"),
                        template: "#= FormatSeriesLabels(value)#",
                        rotation: '270',
                        padding: {
                            left: -5
                        },
                    },
                },
                dataSource: dataSource,
                onDrag: function (e) {
                    var chart = e.sender;
                    var ds = chart.dataSource;
                    var options = chart.options;

                    var categoryRange = e.axisRanges.CategoryAxis;

                    if (categoryRange) {
                        var xMin = categoryRange.min;
                        var xMax = categoryRange.max;
                        options.categoryAxis.min = xMin;
                        options.categoryAxis.max = xMax;
                        ds.filter(viewModel.getFilter(xMin, xMax));
                    }
                },

                pannable: {
                    lock: "y"
                },
                zoomable: {
                    mousewheel: {
                        lock: "y"
                    },
                    selection: {
                        lock: "y"
                    }
                },
                series: series_MonthlyTargetCollectionPartial,
                valueAxis: {
                    line: {
                        visible: true,
                    },
                    labels: {
                        template: "#= FormatSeriesLabels(value)#"
                    },
                    title: { text: amountFigureType }
                },

                seriesColors: colors,
                categoryAxis: {
                    field: $('.btnDataGrid_MonthlyTargetCollectionPartial').data("catagoryAxis"),
                    labels: {
                        rotation: -45,
                    },
                },
                tooltip: {
                    visible: true,
                    format: "{0}",
                    template: "#= category #: \n #= FormatSeriesLabels(value)#"
                },
                dataBound: function (e) {
                    var data = e.sender.dataSource.view();
                    var fieldtype = e.sender.options.series[1].field
                    var max = 0;

                    if (fieldtype == "Target") {

                        jQuery.map(data, function (obj) {
                            if (obj.Target > max)
                                max = obj.Target;
                        });
                        var majorunit = Math.max(6, max / 6);
                        e.sender.options.valueAxis.majorUnit = majorunit;

                        e.sender.options.valueAxis.max = max + majorunit;
                    } else {

                        jQuery.map(data, function (obj) {
                            if (obj.Collection > max)
                                max = obj.Collection;
                        });
                        var majorunit = Math.max(6, max / 6);
                        e.sender.options.valueAxis.majorUnit = majorunit;

                        e.sender.options.valueAxis.max = max + majorunit;
                    }
                },
                seriesClick: function (e) {
                    $('.btnDataGrid_MonthlyTargetCollectionPartial').data("catagoryAxis", "day");
                    var dateformat = $("input[name=DateFormat_TargetCollection]:checked").val();
                    e.dataItem.Month = dateformat == "AD" ? e.dataItem.Month : e.dataItem.Month;
                    if (e.dataItem.Month == undefined || e.dataItem.Month == null)
                        return false;
                    var dataSourcetest = new kendo.data.DataSource({
                        transport: {
                            read: {
                                url: daywiseUrl + "?monthName=" + e.dataItem.Month + "&DateFormat=" + dateformat,
                                dataType: "json", // <-- The default was "jsonp".
                                type: "POST",
                                contentType: "application/json; charset=utf-8"
                            },
                            parameterMap: function (options, type) {
                                var paramMap = JSON.stringify($.extend(options, reportFilter));
                                delete paramMap.$inlinecount; // <-- remove inlinecount parameter.
                                delete paramMap.$format; // <-- remove format parameter.
                                return paramMap;
                            }
                        },
                        requestStart: function () {
                            kendo.ui.progress($("#TargetCollectionChart"), true);
                        },
                        requestEnd: function () {
                            kendo.ui.progress($("#TargetCollectionChart"), false);
                        },
                        sort: {
                            field: "day",
                            dir: "asc"
                        }
                    });
                    var chart = $("#TargetCollectionChart").data("kendoChart");

                    type = $("input[name=seriesType_TargetCollection]:checked").val(),
                    chart.options.series[0].field = "target";
                    chart.options.series[1].field = "Collection";
                    //chart.options.categoryAxis.field = "day";
                    chart.setOptions({
                        seriesColors: ["#5cb85c", "#da3b36", "#e67d4a", "#5bc0de", "#428bca", "#f2b661"],
                        categoryAxis: {
                            field: "day",
                            labels: {
                                rotation: -45,
                            },
                            title: { text: e.category }
                        },
                        tooltip: {
                            template: "day #= category #: \n #= FormatSeriesLabels(value)#"
                        },
                    });
                    chart.setDataSource(dataSourcetest);
                    chart.refresh();
                },
            });
        }
        function refreshMonthlyTargetCollectionChart() {
            if ($('.btnDataGrid_MonthlyTargetCollectionPartial').data("type") == "Grid")// data-grid toogle chart option in data-grid attribute in div
            {
                $('.btnDataGrid_MonthlyTargetCollectionPartial').data("type", "Chart");
                createMonthlyTargetCollectionChart();
                return;
            }
            var chart = $("#TargetCollectionChart").data("kendoChart"),
                type = $("input[name=seriesType_TargetCollection]:checked").val(),
                stack = $("#targetCollectionInStack").prop("checked"),
                IsShowLevel = $("#showlables_monthlyTargetCollection").prop("checked");


            for (var i = 0, length = series_MonthlyTargetCollectionPartial.length; i < length; i++) {
                series_MonthlyTargetCollectionPartial[i].stack = stack;
                series_MonthlyTargetCollectionPartial[i].type = type;
            };

            if (type == "bar") {
                chart.setOptions({
                    series: series_MonthlyTargetCollectionPartial,
                    valueAxis: {
                        labels: {
                            rotation: -45
                        }
                    },
                });
            }
            else {
                chart.setOptions({
                    series: series_MonthlyTargetCollectionPartial,
                });
            }

            chart.options.series[0].labels.visible = IsShowLevel;
            chart.options.series[1].labels.visible = IsShowLevel;
            if (type == "column") {
                //chart.options.series[0].labels.position = "top";
                chart.options.series[0].labels.rotation = -90;
                chart.options.series[1].labels.rotation = -90;
            }
            if (type == "bar") {
                //  chart.options.series[0].labels.position = "center";
                chart.options.series[0].labels.rotation = 0;
                chart.options.series[1].labels.rotation = 0;
            }
            chart.refresh();
        }

        var defaultDateFormat_MonthlyTargetCollection="";
        $('#apply_@actionPageId').click(function () {
            var checkedDateFormat = $('input[name=DateFormat_TargetCollection]:checked').val();
            var IsDateChanged = false;
            if (defaultDateFormat_MonthlyTargetCollection != checkedDateFormat) {
                IsDateChanged = true;
                defaultDateFormat_MonthlyTargetCollection = checkedDateFormat;
            }
            ChartFilter('@actionPageId', "createMonthlyTargetCollectionChart",IsDateChanged);
        });
        $("#showzerovalue_monthlyTargetCollection").on("change", function () {
            createMonthlyTargetCollectionChart();
            RefreshChartFilterControl('@actionPageId');
        });


        //LoadDataGrid function for data-grid
        function LoadDataGrid_MonthlyTargetCollectionPartial(dataSource, monthField) {
            $("#TargetCollectionChart").html('');
            $('.btnDataGrid_MonthlyTargetCollectionPartial').data("type", "Grid");
            var monthTitle = "Month";
            dataSource = dataSource == undefined ? $("#TargetCollectionChart").data("kendoChart").dataSource : dataSource;
            if ($("input[name=DateFormat_TargetCollection]:checked").val() == "BS") {
                if (dataSource.data().length > 0 && dataSource.data()[0].day != null) {
                    monthField = "day";
                    monthTitle = "days [month : " + dataSource.data()[0].Month + "]";
                }
                else {
                    monthField = "MonthYear";
                    monthTitle = "Month"
                }
            }
            if (monthField == undefined)
                if (dataSource.data().length > 0 && dataSource.data()[0].day != null) {
                    monthField = "day";
                    monthTitle = "days [month : " + dataSource.data()[0].Month + "]";
                }
                else {
                    monthField = "MonthYear";
                    monthTitle = "Month"
                }
            $("#TargetCollectionChart").kendoGrid({
                dataSource: dataSource,
                height: $("#MonthlyTargetCollectionPartial").hasClass("portlet-fullscreen") ? 550 : 350,
                groupable: false,
                sortable: true,
                selectable: "multiple",
                toolbar: [{ template: kendo.template($("#toolbar-template").html()) }],
                excel: {
                    fileName: "Monthly Target VS Collection Report",
                    allPages: true
                },
                columns: [{
                    field: monthField,
                    title: monthTitle,
                    width: 240
                }, {
                    field: "Target",
                    title: "Target",
                    format: "{0:n}",
                    attributes: {
                        style: "text-align: right;"
                    },
                }, {
                    field: "Collection",
                    title: "Collection",
                    format: "{0:n}",
                    attributes: {
                        style: "text-align: right;"
                    },
                }]

            });

        }


        function ChartData_MonthlyTargetCollectionPartial() {
            $('.btnDataGrid_MonthlyTargetCollectionPartial').data("type", "Chart");
            createMonthlyTargetCollectionChart();
            RefreshChartFilterControl('@actionPageId');
        };

        $(".btnDataGrid_MonthlyTargetCollectionPartial").click(function () {
            var datasource = $("#TargetCollectionChart").data("kendoChart").dataSource;
            var type = $('.btnDataGrid_MonthlyTargetCollectionPartial').data("type");
            if(type=="Chart"){
                LoadDataGrid_MonthlyTargetCollectionPartial(datasource);
                $('.btnDataGrid_MonthlyTargetCollectionPartial').data("type", "Grid");
            }
            else {
                //ChartData_MonthlyTargetCollectionPartial(datasource);
                LoadMonthlyTargetCollectionChart(datasource);
                $('.btnDataGrid_MonthlyTargetCollectionPartial').data("type", "Chart");
            }
            //$('.btnDataGrid_MonthlyTargetCollectionPartial').data("type") == "Chart" ? LoadDataGrid_MonthlyTargetCollectionPartial() : ChartData_MonthlyTargetCollectionPartial();;
            $("i", this).toggleClass("icon-bar-chart");
        });

        //load ChildGrid when dblClick on row
        $("#TargetCollectionChart").on('dblclick', ".k-state-selected", function (e) {
            var grid = $("#TargetCollectionChart").data("kendoGrid");
            var selectedItem = grid.dataItem(grid.select());
            var dateformat = $("input[name=DateFormat_TargetCollection]:checked").val();
           
            if (selectedItem.Month != null) {
                var daywiseUrl = window.location.protocol + "//" + window.location.host + "/api/SalesDashboard/GettargetCollectionDailyReport";
                var dataSourcetest = new kendo.data.DataSource({
                    transport: {
                        read: {
                            url: daywiseUrl + "?monthName=" + selectedItem.Month + "&DateFormat=" + dateformat,
                            dataType: "json", // <-- The default was "jsonp".
                            type: "POST",
                            contentType: "application/json; charset=utf-8"
                        },
                        parameterMap: function (options, type) {
                            var paramMap = JSON.stringify($.extend(options, reportFilter));
                            delete paramMap.$inlinecount; // <-- remove inlinecount parameter.
                            delete paramMap.$format; // <-- remove format parameter.
                            return paramMap;
                        }
                    },
                    sort: {
                        field: "day",
                        dir: "asc"
                    }
                });
                //
                $("#TargetCollectionChart").html('');
                $("#TargetCollectionChart").kendoGrid({
                    dataSource: dataSourcetest,
                    height: ($("#TargetCollectionChart").hasClass("portlet-fullscreen")) ? 550 : 350,
                    groupable: false,
                    sortable: true,
                    selectable: "multiple",
                    toolbar: [{ template: kendo.template($("#toolbar-template").html()) }],
                    excel: {
                        fileName: "Monthly Target VS Collection Report (" + selectedItem.Month + ")",
                        allPages: true
                    },
                    columns: [{
                        field: "day",
                        title: "days [month : " + selectedItem.Month + "]",
                    },
                    {
                        field: "Target",
                        title: "Target",
                        format: "{0:n}",
                        attributes: {
                            style: "text-align: right;"
                        },
                    }, {
                        field: "Collection",
                        title: "Collection",
                        format: "{0:n}",
                        attributes: {
                            style: "text-align: right;"
                        },
                    }]
                });
            }
        })
    </script>
    <script>

        $(document).ready(function () {
            defaultDateFormat_MonthlyTargetCollection = $('input[name=DateFormat_TargetCollection]:checked').val();
            $('[title="Download"]').tooltip({
                trigger: 'hover'
            });
            $('.icon-arrow-down.Firstcustom').click(function () {
                $('.dropdown-menu.custom.Firstslidetoggle').slideToggle(200);
            });

            ReportFilter.init('@actionPageId');

            $("checkbox").not("#targetCollectionInStack").uniform();
            createMonthlyTargetCollectionChart();
            $(document).bind("kendo:skinChange", createMonthlyTargetCollectionChart);


            $(".target-collection-options").bind("change", function () {
                if ($('.btnDataGrid_MonthlyTargetCollectionPartial').data("type") == "Chart")
                    refreshMonthlyTargetCollectionChart();
                else
                    chart_grid_option_change("btnDataGrid_MonthlyTargetCollectionPartial", 'createMonthlyTargetCollectionChart');
            });
            $("#showlables_monthlyTargetCollection").bind("change", function () {
                var temp = $('.btnDataGrid_MonthlyTargetCollectionPartial').data("type");
                $('.btnDataGrid_MonthlyTargetCollectionPartial').data("type", "temp");
                refreshMonthlyTargetCollectionChart();
                $('.btnDataGrid_MonthlyTargetCollectionPartial').data("type", temp);
            });
            $('#targetCollectionInStack').on('click', function () {
                var temp = $('.btnDataGrid_MonthlyTargetCollectionPartial').data("type");
                $('.btnDataGrid_MonthlyTargetCollectionPartial').data("type", "temp");
                refreshMonthlyTargetCollectionChart();
                $('.btnDataGrid_MonthlyTargetCollectionPartial').data("type", temp);
