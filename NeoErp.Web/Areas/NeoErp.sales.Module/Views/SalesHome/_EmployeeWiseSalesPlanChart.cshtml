                    </div>
                </div>
                <div class="br"></div>


                <div class="filter_title" >
                    <label class="filter-label">Advance Filter</label>
                </div>

                @Html.Partial("~/Views/Shared/Controls/DashboardChartFilter/_AdvancedFilter.cshtml", new AdvancedFilterSettingsModel()
                {
                    ShowCustomerFilter = true,
                    ShowProductFilter = false,
                    ShowPartyTypeFilter = false,
                    ShowCategoryFilter = false,
                    ShowLocationFilter = false,
                    ShowDocumentFilter = false,
                    ShowCompanyFilter = false,
                    ShowFiscalYearFilter = false,
                    ShowBranchFilter = true,
                    ShowEmployeeFilter = true,
                    ShowAgentFilter = false,
                    ShowDivisionFilter = true,
                    IsPopUp = false,
                    ActionPageId = actionPageId
                })
            </div>


            <div class="modal-footer">
                @*<button type="button" class="btn default" data-dismiss="modal">Close</button>*@
                <button type="button" class="btn btn-green applydp" data-dismiss="modal" id="apply_@actionPageId">Apply</button>
            </div>
        </div>
        <!-- /.modal-content -->
    </div>
    <!-- /.modal-dialog -->
</div>

<div class="portlet portlet-sortable  light bordered" id="EmployeeWiseSalesPlan">
    <div class="portlet-title">
        <div class="caption">
            <i class="icon-bar-chart font-green-haze"></i>
            <span class="caption-subject bold uppercase font-green-haze custom-modal">Employee wise Sales plan</span>
        </div>
        <div class="tools">
            <a href="javascript:;" class="collapse"></a>
            @*<a class="btnDatagrid" href="javascript:;" onclick="CreateGrid_EmployeeWiseSalesPlan();" data-grid="Chart" title="Data grid"> *@
            <a class="btnGrid btnGrid_EmployeeWiseSalesPlan" data-type="Chart" data-title="Grid">
                <i class="fa fa-table"></i>
            </a>
            <a class="" id="btnBackEmployeeWiseSalesPlan" style="display:none">
                <i class="fa fa-angle-double-left"></i>
            </a>
            <div class="btn-group">
                <a class="btn dropdown-toggle download-btn" data-title="Download" data-toggle="dropdown" href="#">
                    <i class="icon-arrow-down Thirdcustom"></i>
                </a>
                <ul class="dropdown-menu custom">
                    <div class="btn-group-custom">
                        <button type="button" data-id="EmployeeWiseSalesPlanChart" class="export-pdf btn default red-stripe"><i class="fa fa-file-pdf-o"></i>PDF</button>
                        <button type="button" data-id="EmployeeWiseSalesPlanChart" class="export-img btn default blue-stripe"><i class="fa fa-image-o"></i>Image</button>
                        <button type="button" data-id="EmployeeWiseSalesPlanChart" class="export-svg btn default green-stripe">SVG</button>
                    </div>
                </ul>
            </div>
            <a href="#Model_@actionPageId" data-toggle="modal" class="config"></a>
            <a href="javascript:;" class="reload"></a>
            <a href="javascript:;" class="fullscreen"></a>
            <a href="javascript:;" class="remove"></a>
        </div>
    </div>
    <div class="portlet-body">
        <script type="text/x-kendo-template" id="toolbar-template">
            <div class="k-header k-grid-toolbar pull-right">

                <a class="k-button k-button-icontext k-grid-excel" href="javascript:">
                    <i class="fa fa-file-excel-o"></i>
                </a>
        </script>
        <div class="k-content wide">
            <div id="EmployeeWiseSalesPlanChart"></div>
            <div class="overlay"><div>No data available</div></div>
            <div class="AmountFigureType"></div>
            <div class="DisplayFilterContent"><a href="javascript:;" data-toggle="tooltip" title=""></a></div>
        </div>
    </div>

    <script>
        //initialize variables
        var config_EmployeeWiseSalesPlan = {
            url: window.location.protocol + "//" + window.location.host + "/api/PlanChartReportApi/getEmployeeWiseSalesPlanChart",
            childurl: window.location.protocol + "//" + window.location.host + "/api/PlanChartReportApi/getEmployeeCustomerWiseSalesPlanChart",
            dateFormat: $("input[name=DateFormat_EmployeeWiseSalesPlan]:checked").val(),
            fieldValue: $("input[name=fieldType_EmployeeWiseSalesPlan]:checked").val(),
            series: [{
                field: "PER_DAY_AMOUNT",
                name: "GrossAmount"//"#= group.value #"
            }],
            isStack: false,
            showLabel: $("#showLabel_EmployeeWiseSalesPlan").prop("checked"),
            color: ["#8A2BE2", "#ADFF2F", "#5bc0de", "#5cb85c", "#f2b661"],
            seriesType: $("input[name=seriesType_EmployeeWiseSalesPlan]:checked").val(),
            categoryField: "DESCRIPTION",
            categoryTitle: '',
            group: { field: "COMPANY_CODE" },
            sortField: "MONTHINT",
            sortFields: [{
                field: "YEAR",
                dir: "asc"
            }, {
                field: "MONTHINT",
                dir: "asc"
            }],
            _oldData: {
                dateFormat: $("input[name=DateFormat_EmployeeWiseSalesPlan]:checked").val(),
                fieldValue: $("input[name=fieldType_EmployeeWiseSalesPlan]:checked").val()
            },

        };
        var _oldConfig_EmployeeWiseSalesPlan = JSON.stringify(config_EmployeeWiseSalesPlan);



        function loadDataSource_EmployeeWiseSalesPlan() {
            var url1;
            var param = {};
            if (isChild == undefined) {
                url1 = config_EmployeeWiseSalesPlan.url;
                param = {
                    DateFormat: config_EmployeeWiseSalesPlan.dateFormat
                };
            }
            else {
                url1 = config_EmployeeWiseSalesPlan.childurl;
                param = { DateFormat: config_EmployeeWiseSalesPlan.dateFormat, EmpCode: EmployeeCode };

            }
            reportFilter = reportFilter == undefined ? ReportFilter.filterAdditionalData() : reportFilter;
            var filter = $.extend(true, {}, reportFilter);
            config_EmployeeWiseSalesPlan.dateFormat = $("input[name=DateFormat_EmployeeWiseSalesPlan]:checked").val();
            config_EmployeeWiseSalesPlan.showLabel = $("#showLabel_EmployeeWiseSalesPlan").prop("checked");
            var dataSource = new kendo.data.DataSource({
                transport: {
                    read: {
                        url:Arg.url(url1,param),
                        //url: Arg.url(config_EmployeeWiseSalesPlan.url, { DateFormat: config_EmployeeWiseSalesPlan.dateFormat }),
                        dataType: "json", // <-- The default was "jsonp".
                        type: "POST",
                        contentType: "application/json; charset=utf-8"
                    },
                    parameterMap: function (options, type) {
                        var paramMap = JSON.stringify($.extend(options, filter));
                        delete paramMap.$inlinecount; // <-- remove inlinecount parameter.
                        delete paramMap.$format; // <-- remove format parameter.
                        return paramMap;
                    }
                },
                requestStart: function () {
                    if (IsDataGrid != "Grid") {
                        kendo.ui.progress($("#EmployeeWiseSalesPlanChart"), true);
                    }
                },
                requestEnd: function () {
                    kendo.ui.progress($("#EmployeeWiseSalesPlanChart"), false);
                },
                schema: {
                    parse: function (items) {
                        var item,
                            result = [];
                        var showzerovalue = $("#showzerovalue_EmployeeWiseSalesPlan").prop("checked");

                        if (showzerovalue) {
                            for (var i = 0; i < items.length; i++) {
                                item = items[i];
                                result.push(item);
                            }
                        }
                        else {
                            for (var i = 0; i < items.length; i++) {
                                item = items[i];
                                if (item.Amount !== 0 && item.Quantity !== 0) {
                                    result.push(item);
                                }
                            }
                        }


                        return result;
                    }
                },
                group: config_EmployeeWiseSalesPlan.group,
                sort: config_EmployeeWiseSalesPlan.sortFields
                //{
                //    field: config_EmployeeWiseSalesPlan.sortField,
                //    dir: "asc"
                //}

            });

            $("#EmployeeWiseSalesPlan .DisplayFilterContent a").text("");
            var IsDataGrid = $('.btnGrid_EmployeeWiseSalesPlan').data("type");
            if (IsDataGrid == "Grid") {
                CreateGrid_EmployeeWiseSalesPlan(dataSource);
                return;
            }

            $('.btnGrid_EmployeeWiseSalesPlan').data("type", "Chart");
            CreateChart_EmployeeWiseSalesPlan(dataSource);
            //CreateGrid_EmployeeWiseSalesPlan(dataSource);
        }
        function CreateChart_EmployeeWiseSalesPlan(dataSource) {
          var  amountorquantity = $("input[name=fieldType_EmployeeWiseSalesPlan]:checked").val();
            var templa = amountorquantity == "Amount" ? "#= FormatSeriesLabels(value)#" : "#= FormatLabels(value)#"

            $('.btnGrid_EmployeeWiseSalesPlan').data("type", "Chart");
            $("#EmployeeWiseSalesPlanChart").html('');
            $("#EmployeeWiseSalesPlanChart").removeAttr("style");
            $("#EmployeeWiseSalesPlanChart").removeAttr("class");
            dataSource.group(config_EmployeeWiseSalesPlan.group);
            $("#EmployeeWiseSalesPlanChart").kendoChart({
                chartArea: {
                    height: ($("#EmployeeWiseSalesPlanChart").hasClass("portlet-fullscreen")) ? 550 : 350,
                },
                onDrag: function (e) {
                    var chart = e.sender;
                    var ds = chart.dataSource;
                    var options = chart.options;

                    var categoryRange = e.axisRanges.CategoryAxis;

                    if (categoryRange) {
                        var xMin = categoryRange.min;
                        var xMax = categoryRange.max;

                        options.categoryAxis.min = xMin;
                        options.categoryAxis.max = xMax;

                        ds.filter(viewModel.getFilter(xMin, xMax));
                    }
                },
                pannable: {
                    lock: "y"
                },
                zoomable: {
                    mousewheel: {
                        lock: "y"
                    },
                    selection: {
                        lock: "y"
                    }
                },
                dataSource: dataSource,
                legend: {
                    position: "bottom",
                    visible:false
                },
                seriesColors: config_EmployeeWiseSalesPlan.color,
                seriesDefaults: {
                    type: config_EmployeeWiseSalesPlan.seriesType,
                    labels: {
                        visible: config_EmployeeWiseSalesPlan.showLabel,
                        template: templa,
                        rotation: '270',
                        padding: {
                            left: -5
                        },
                    },
                    style: "smooth",
                },
                series: config_EmployeeWiseSalesPlan.series,
                categoryAxis: {
                    field: config_EmployeeWiseSalesPlan.categoryField,
                    labels: {
                        rotation: -45,
                    },
                    title: { text: $("#EmployeeWiseSalesPlanChart").data("kendoChart") == undefined ? "" : $("#EmployeeWiseSalesPlanChart").data("kendoChart").options.categoryAxis.title.text }
                    //  text: config_EmployeeWiseSalesPlan.categoryTitle
                },
                valueAxis: {
                    line: {
                        visible: true
                    },
                    labels: {
                        rotation: 'auto',
                        template: templa
                    },
                    title: { text: amountFigureType }
                },
                tooltip: {
                    visible: true,
                    format: "{0}",
                    template: "#= category #: \n" + templa + "\n #=drillDownToolTipMessage(category) #"
                },
                dataBound: function (e) {
                    var maxValue = getMaxValueFromDataSource(e);
                    var majorunit = Math.max(6, maxValue / 6);
                    e.sender.options.valueAxis.majorUnit = majorunit;
                    e.sender.options.valueAxis.max = maxValue + majorunit;
                    $("#EmployeeWiseSalesPlanChart").next(".overlay").toggle(maxValue <= 0);
                    e.sender.options.categoryAxis.categories = getCategoryFromDataSource(e);
                    //
                    //if (totalChartList.length > 0) {
                    //    var chart = totalChartList[0];
                    //    totalChartList = totalChartList.splice(1, totalChartList.length);
                    //    loadChartSync(chart);
                    //}
                },
               //added by chandra for series click
                seriesClick: function (e) {
                    //reportFilter = reportFilter == undefined ? ReportFilter.filterAdditionalData() : reportFilter;
                   // var filter = $.extend(true, {}, reportFilter);
                    var value = e.dataItem.EMPLOYEE_CODE;
                    if (value == null) {
                        kendo.ui.progress($("#EmployeeWiseSalesPlanChart"), false);
                        return false;
                    }
                    if (value == "NaN" || "") {
                        kendo.ui.progress($("#EmployeeWiseSalesPlanChart"), false);
                        return;
                    }
                    isChild = true;
                    EmployeeCode = value;
                    $('#btnBackEmployeeWiseSalesPlan').show();
                    kendo.ui.progress($("#EmployeeWiseSalesPlanChart"), true);
                    loadDataSource_EmployeeWiseSalesPlan();
                    var chart = $("#EmployeeWiseSalesPlanChart").data("kendoChart");
                    chart.setOptions({
                        categoryAxis: {
                            title: { text: e.category }
                        },
                    });
                    kendo.ui.progress($("#EmployeeWiseSalesPlanChart"), false);
                },
                //end added by chandra
            });
        }
        function drillDownToolTipMessage(text) {
            if (isChild == undefined || isChild == 'undefined') {
                return '<br/><h6>click to show Customer wise sales plan of Employee: ' + text+'</h6>';
            }
            return "";
        }
        function CreateGrid_EmployeeWiseSalesPlan(dataSource) {

            $("#EmployeeWiseSalesPlanChart").html('');
            $("#EmployeeWiseSalesPlanChart").removeAttr("style");
            $("#EmployeeWiseSalesPlanChart").removeAttr("class");
            $('.btnGrid_EmployeeWiseSalesPlan').data("type", "Grid");
            var format = $("input[name=DateFormat_EmployeeWiseSalesPlan]:checked").val();
            var monthTitle = "Month";
            dataSource = dataSource == undefined ? $("#EmployeeWiseSalesPlanChart").data("kendoChart").dataSource : dataSource;
            if (dataSource.data().length > 0 && dataSource.data()[0].Day != null) {
                monthField = "Day";
                monthTitle = "Days [month : " + dataSource.data()[0].DisplayMonth + "]";
            }
            else {
                monthField = config_EmployeeWiseSalesPlan.categoryField;
                monthTitle = "Month"
            }
            dataSource.group([]);
            $("#EmployeeWiseSalesPlanChart").kendoGrid({
                dataSource: dataSource,
                height: $("#EmployeeWiseSalesPlanChart").hasClass("portlet-fullscreen") ? 550 : 350,
                groupable: false,
                sortable: true,
                //selectable: "multiple",
                toolbar: [{ template: kendo.template($("#toolbar-template").html()) }],
                excel: {
                    fileName: "Sales Plan Report",
                    allPages: true
                },
                columns: [{
                    field: monthField,
                    title: isChild == undefined ? "Employee" : "Customer",
                    width: 240
                }, {
                    field: "PER_DAY_QUANTITY",
                    title: "Quantity",
                    format: "{0:n0}",
                    attributes:
                    {
                        style: "text-align:right;"
                    },

                }, {
                    field: "PER_DAY_AMOUNT",
                    title: "Amount",
                    format: "{0:n}",
                    attributes:
                    {
                        style: "text-align:right;"
                    },

                    }, {
                        field: "EMPLOYEE_EDESC",
                        title: "Employee",
                        //width:  240,
                        hidden: isChild ? false : true
                    }],
                dataBound: function (e) {
                    var gird = e.sender;
                    gird.table.find(".k-grouping-row:not(:first-child)").each(function () {
                        gird.collapseGroup(this);
                    });
                }
            });
        }



        function refreshEmployeeWiseSalesPlan() {
            if ($(".btnGrid_EmployeeWiseSalesPlan").data("type") == "Grid")// data-grid toogle chart option in data-grid attribute in div
            {
                $(".btnGrid_EmployeeWiseSalesPlan").data("type", "Chart");
                loadDataSource_EmployeeWiseSalesPlan();
                return;
            }

            var chart = $("#EmployeeWiseSalesPlanChart").data("kendoChart"),
                type = $("input[name=seriesType_EmployeeWiseSalesPlan]:checked").val(),
                stack = $("#Stack_EmployeeWiseSalesPlan").prop("checked"),
                IsLebelShow = $("#showLabel_EmployeeWiseSalesPlan").prop("checked"),
                fieldValue = $("input[name=fieldType_EmployeeWiseSalesPlan]:checked").val();

            for (var i = 0, length = config_EmployeeWiseSalesPlan.series.length; i < length; i++) {
                config_EmployeeWiseSalesPlan.series[i].stack = stack;
                config_EmployeeWiseSalesPlan.series[i].type = type;
                if (fieldValue == "Amount")
                    config_EmployeeWiseSalesPlan.series[i].field = "PER_DAY_AMOUNT";
                else
                    config_EmployeeWiseSalesPlan.series[i].field = "PER_DAY_QUANTITY";

            };

            chart.setOptions({
                series: config_EmployeeWiseSalesPlan.series,
            });

            for (var i = 0; i < chart.options.series.length; i++) {
                chart.options.series[i].labels.visible = IsLebelShow;
                if (type == "column") {
                    chart.options.series[i].labels.rotation = -90;
                }
                if (type == "bar") {
                    chart.options.series[i].labels.rotation = 0;
                }
            }

            chart.refresh();
        }
        $('#apply_EmployeeWiseSalesPlan').click(function () {
            var checkedDateFormat = $("input[name=DateFormat_EmployeeWiseSalesPlan]:checked").val();
            var checkedFieldValue = $("input[name=fieldType_EmployeeWiseSalesPlan]:checked").val();
            var IsChanged = false;
            if (config_EmployeeWiseSalesPlan._oldData.dateFormat != checkedDateFormat) {
                IsChanged = true;
                config_EmployeeWiseSalesPlan = JSON.parse(_oldConfig_EmployeeWiseSalesPlan);
                config_EmployeeWiseSalesPlan._oldData.dateFormat = checkedDateFormat;

            }
            if (config_EmployeeWiseSalesPlan._oldData.fieldValue != checkedFieldValue) {
                refreshEmployeeWiseSalesPlan();
                config_EmployeeWiseSalesPlan._oldData.fieldValue = checkedFieldValue;
            }
            $('.btnGrid_EmployeeWiseSalesPlan').data("type", "Chart");
            ChartFilter('EmployeeWiseSalesPlan', "loadDataSource_EmployeeWiseSalesPlan", IsChanged);
        });
        var isChild = undefined;
        var EmployeeCode=0;
        $(document).ready(function () {
            loadDataSource_EmployeeWiseSalesPlan();

            //start Binding
            $(document).bind("kendo:skinChange", loadDataSource_EmployeeWiseSalesPlan);

            $('#EmployeeWiseSalesPlanChart').closest('.portlet').find('.reload').click(function () {
                RefreshChartFilterControl('EmployeeWiseSalesPlan');
                $('.btnGrid_EmployeeWiseSalesPlan').data("type", "Chart");
                config_EmployeeWiseSalesPlan = JSON.parse(_oldConfig_EmployeeWiseSalesPlan);
                loadDataSource_EmployeeWiseSalesPlan();
            });



            $(".btnGrid_EmployeeWiseSalesPlan").click(function () {
                var datasource = $("#EmployeeWiseSalesPlanChart").data("kendoChart").dataSource;
                $('.btnGrid_EmployeeWiseSalesPlan').data("type") == "Chart" ? CreateGrid_EmployeeWiseSalesPlan(datasource) : CreateChart_EmployeeWiseSalesPlan(datasource);
                $("i", this).toggleClass("icon-bar-chart");
            });
            $(".options_EmployeeWiseSalesPlan").bind("change", function () {  // data-grid toogle chart option in data-grid attribute in button
                if ($('.btnGrid_EmployeeWiseSalesPlan').data("type") == "Chart")
                    refreshEmployeeWiseSalesPlan();
                else
                    chart_grid_option_change("btnGrid_EmployeeWiseSalesPlan", "loadDataSource_EmployeeWiseSalesPlan");
            });
            $("#showLabel_EmployeeWiseSalesPlan").bind("change", function () {// data-grid toogle chart option in data-grid attribute in button
                var temp = $('.btnGrid_EmployeeWiseSalesPlan').data("type");
                $('.btnGrid_EmployeeWiseSalesPlan').data("type", "temp");
                refreshEmployeeWiseSalesPlan()
                $('.btnGrid_EmployeeWiseSalesPlan').data("type", temp);
            });
            $('#Stack_EmployeeWiseSalesPlan').on('click', function () {// data-grid toogle chart option in data-grid attribute in button
                var temp = $('.btnGrid_EmployeeWiseSalesPlan').data("type");
                $('.btnGrid_EmployeeWiseSalesPlan').data("type", "temp");
                refreshEmployeeWiseSalesPlan()
                $('.btnGrid_EmployeeWiseSalesPlan').data("type", temp);
            });

        });

        function FormatSeriesLabels(value) {
            if (isNaN(value))
                value = 0;
            var decimalPlaces = 2;
            if (ReportFilter.filterAdditionalData().ReportFilters.AmountRoundUpFilter != "" && ReportFilter.filterAdditionalData().ReportFilters.AmountRoundUpFilter != undefined) {
                if (ReportFilter.filterAdditionalData().ReportFilters.AmountRoundUpFilter.split(".").length > 1)
                    decimalPlaces = ReportFilter.filterAdditionalData().ReportFilters.AmountRoundUpFilter.split(".")[1].length;
                else
                    decimalPlaces = 0;
            }

            if (ReportFilter.filterAdditionalData().ReportFilters.AmountFigureFilter == "Lakh") {
                value = (value / 100000).toFixed(decimalPlaces) + " L";
            } else if (ReportFilter.filterAdditionalData().ReportFilters.AmountFigureFilter == "Thousand") {
                value = (value / 1000).toFixed(decimalPlaces) + " K";
            }
            else if (ReportFilter.filterAdditionalData().ReportFilters.AmountFigureFilter == "Crore") {
                value = (value / 10000000).toFixed(decimalPlaces) + " C";
            }
            else {
                value = (value / 100000).toFixed(decimalPlaces) + " L";
            }
            return kendo.format("{0:n" + ReportFilter.parseDecimalPlace("AmountRoundUpFilter").toString() + "}", value);
        }
        $('#btnBackEmployeeWiseSalesPlan').click(function () {
            isChild = undefined;
            $('#btnBackEmployeeWiseSalesPlan').hide();
            var chart = $("#EmployeeWiseSalesPlanChart").data("kendoChart");
            chart.setOptions({
                categoryAxis: {
                    title: { text: "" }
                },
            });
            $(".btnGrid_EmployeeWiseSalesPlan").data("type", "Chart");
            $('.btnGrid_EmployeeWiseSalesPlan').children().removeClass().addClass('fa fa-table');
            loadDataSource_EmployeeWiseSalesPlan();
            
           // $('.btnGrid_EmployeeWiseSalesPlan').data('type') == 'Grid' ? refreshEmployeeWiseSalesPlan() : loadDataSource_EmployeeWiseSalesPlan();
        })
    </script>
</div>
