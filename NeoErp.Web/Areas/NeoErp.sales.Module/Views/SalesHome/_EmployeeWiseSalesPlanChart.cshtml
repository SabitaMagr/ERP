@using NeoErp.Core.Models;
@{
    var actionPageId = "EmployeeWiseSalesPlan";
}

<div class="modal fade" id="Model_@actionPageId" tabindex="-1" role="dialog" aria-labelledby="myModalLabel" aria-hidden="true">
