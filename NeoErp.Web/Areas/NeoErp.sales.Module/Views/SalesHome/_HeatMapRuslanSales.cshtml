        function handleFileSelectCSVHeatMapRuslanSales(evt) {


            var file = evt.target.files[0];

            Papa.parse(file, {
                header: true,
                dynamicTyping: true,
                complete: function (results) {
                    csv = [];

                    if (results.meta.fields.indexOf("weight") == -1) {
                        for (idx in results["data"]) {
                            var row = results["data"][idx];
                            csvHeatMapRuslanSales.push(new google.maps.LatLng(row["lat"], row["lon"]))
                        }
                    } else {
                        var max = results["data"][0]["weight"];

                        for (idx in results["data"]) {
                            var row = results["data"][idx];

                            max = Math.max(max, row["weight"]);

                            csvHeatMapRuslanSales.push({
                                location: new google.maps.LatLng(row["lat"], row["lon"]),
                                weight: row["weight"]
                            });
                        }

                        $("#max-label_@actionPageId").html("max: " + numberWithCommas(max));
                        $("#max-slider_@actionPageId").slider("option", "max", max);
                        $("#max-slider_@actionPageId").slider("option", "value", max);
                    }

                    console.log(results);

                    loadHeatmapHeatMapRuslanSales(csv);
                }
            });
        }

        function initializeHeatMapRuslanSales() {
            var mapOptions = {
                zoom: 7,
                fullscreenControl: true,
                //minZoom: 7,
                //maxZoom: 25,
                center: new google.maps.LatLng(28.43443095421775, 84.20351049414055),
                mapTypeId: google.maps.MapTypeId.ROADMAP
            };

            mapHeatMapRuslanSales = new google.maps.Map(document.getElementById('map-canvas_@actionPageId'),
                mapOptions);
        }

        function loadHeatmapHeatMapRuslanSales(csvHeatMapRuslanSales) {
            var pointArray = new google.maps.MVCArray(csvHeatMapRuslanSales);

            if (heatmapHeatMapRuslanSales) heatmapHeatMapRuslanSales.setMap(null);

            heatmapHeatMapRuslanSales = new google.maps.visualization.HeatmapLayer({
                data: pointArray,
                radius: $("#radius-slider_@actionPageId").slider("value"),
                opacity: $("#opacity-slider_@actionPageId").slider("value")
            });

            heatmapHeatMapRuslanSales.setMap(mapHeatMapRuslanSales);
        }

