		function numberWithCommas(x) {
				return x.toString().replace(/\B(?=(\d{3})+(?!\d))/g, ",");
		}

        function handleFileHeatMapRuslanSales(evt) {
            $.param({ Company_code: '@companyCode', DivisoinCode: '@DivisionCode'})
