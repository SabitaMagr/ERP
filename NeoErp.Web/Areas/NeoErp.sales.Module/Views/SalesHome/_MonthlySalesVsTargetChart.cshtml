            <a href="javascript:;" class="collapse"></a>
            @*<a class="btnDatagrid" href="javascript:;" onclick="CreateGrid_MonthlySalesPlanChart();" data-grid="Chart" title="Data grid">*@
            <a class="btnGrid btnGrid_MonthlySalesVsTargetChart" data-type="Chart" data-title="Grid">
