                            DISCOUNT_AMT: { type: "number" },
                            DISCOUNTED_AMOUNT: { type: "number" },
                            Special_Discount_Scheme: { type: "number" },
                            EXCISE_DUTY: { type: "number" },
                            ITEM_SUBGROUP_EDESC: { type: "string" },
