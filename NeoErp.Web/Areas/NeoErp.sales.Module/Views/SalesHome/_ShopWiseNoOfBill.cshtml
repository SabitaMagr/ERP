            $("#ShopWise_NoOfBill").kendoGrid({
                dataSource: dataSource == undefined ? $("#ShopWise_NoOfBill").data("kendoChart").dataSource : dataSource,
                //height: $("#ShopWise_NoOfBill").hasClass("portlet-fullscreen") ? 550 : 350,
                groupable: false,
                sortable: true,
                toolbar: [{ template: kendo.template($("#toolbar-template").html()) }],
                excel: {
                    fileName: excelName,
                    allPages: true
                },
               // selectable: "multiple",
                columns: [{
                    field: "Description",
                    title: column,
                    width: 240
                }, {
                    field: "Quantity",
                    title: "Quantity",
                    format: "{0:n0}",
                    attributes: {
                        style: "text-align: right;"
                    },
                }, {
                    field: "Total",
                    title: "Total",
                    format: "{0:n}",
                    attributes: {
                        style: "text-align: right;"
                    },
                }]
            });
        }

        function GenerateDataGrid_Category(datasource) { // data-grid function onClick event of button DataGrid
            
            $('.btnCategoryDatagrid').data("type", "Grid");
            if (datasource == undefined || datasource == null) {
                datasource = $("#ShopWise_NoOfBill").data("kendoChart").dataSource;
            }
            LoadDataGrid_Category(datasource);
        }

