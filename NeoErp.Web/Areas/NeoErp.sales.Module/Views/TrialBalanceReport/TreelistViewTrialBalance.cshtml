                         url: '@Url.Action("LedgerSummaryReport", "Ledger")',
                        data: data,
                        cache: false,
                        type: "POST",
                        dataType: "html",
                        success: function (data, textStatus, XMLHttpRequest) {
                            $(activeTab).html('');
