    function BindGrid(readurl, refresh) {
        var dataSource = new kendo.data.TreeListDataSource({
            transport: {
                read: {
                    url: readurl, // <-- Get data from here.
                    dataType: "json", // <-- The default was "jsonp".
                    type: "POST",
                    //contentType: "application/json",
                    data: function (data, evt) {
                        var datageneric = $("#datageneric option:selected").val();
                        var treeList = $("#grid").data("kendoTreeList");
                        var dataSource = treeList.dataSource;
                        if (data.id) {
                            debugger;
                            var parentNode = dataSource.get(data.id);
                            return $.extend(ReportFilter.filterAdditionalData(), { parentId: parentNode.ACC_CODE, subledger: parentNode.sub_count, datatype: datageneric })

                        } else {
                            debugger;
                            return $.extend(ReportFilter.filterAdditionalData(), { datatype: datageneric })
                        }

                    }
                }
            },

            schema: {
                model: {
                    id: "MasterId",
                    parentId: "ReportsTo",
                    fields: {
                        MasterId: { type: "number", nullable: false, field: "Id" },
                        ReportsTo: { field: "ParentIdInt", nullable: false, type: "number" },
                    }
                }
            },
            aggregate: [
                           { field: "DR_OPENING", aggregate: "sum" },
                           { field: "CR_OPENING", aggregate: "sum" },
                             { field: "DR_AMT", aggregate: "sum" },
                           { field: "CR_AMT", aggregate: "sum" },
                            { field: "DR_CLOSING", aggregate: "sum" },
                           { field: "CR_CLOSING", aggregate: "sum" },

            ]

        });
        if (refresh) {
            var grid = $('#grid').data('kendoTreeList');
            grid.setDataSource(dataSource);
            grid.refresh();

        }
        else {
            $("#grid").kendoTreeList({
                dataSource: dataSource,
                dataBound: onDataBoundtest,
                selectable: true,
                change: function () {
                    var text = "";
                    var grid = this;

                    grid.select().each(function () {
                        var dataItem = grid.dataItem($(this));
                        $("#subledgerAccountNo").val(dataItem.Sub_code);
                        $("#ledgerAccountNo").val(dataItem.ACC_CODE);
                        $("#ledgerAccountName").val(dataItem.account_head);
                    });
                },
                //adding column for the subledger menu
                columnMenu: true,
                columnMenuInit: function (e) {
                    wordwrapmenu(e);
                },

                //end the adding  column part
                columns: [

                {
                    field: "account_head",
                    title: "Account Name",
                    width: "35%",
                    footerTemplate: '<span style="float:right;font-weight: bolder;">Total Amount</span>'
                },
                 {
                     field: "DR_OPENING",
                     title: "Dr Amount",
                     // width: "80px",
                     attributes: {
                         style: "text-align: right;"
                     },
                     format: "{0:n}",
                     //footerTemplate: "#=sum#"
                     //template: '<span style="float:right">#= kendo.toString(DR_OPENING, "n") #</span>'
                     //aggregates: "sum",
                     footerTemplate: "<div style='float: right;font-weight: bolder;'>#: kendo.format('{0:n}', sum)#</div>"
                 },
                    {
                        field: "CR_OPENING",
                        title: "Cr Amount",
                        // width: "80px",
                        attributes: {
                            style: "text-align: right;"
                        },
                        format: "{0:n}",
                        footerTemplate: "<div style='float: right;font-weight: bolder;'>#: kendo.format('{0:n}', sum)#</div>"
                        //  footerTemplate: "<div style='float: right'>#=sum#</div>"
                        //template: '<span style="float:right">#= kendo.toString(CR_OPENING, "n") #</span>'
                    }, {
                        field: "DR_AMT",
                        title: "Dr Amount",
                        //  width: "80px",
                        attributes: {
                            style: "text-align: right;"
                        },
                        format: "{0:n}",
                        footerTemplate: "<div style='float: right;font-weight: bolder;'>#: kendo.format('{0:n}', sum)#</div>"
                        //  footerTemplate: "<div style='float: right'>#=sum#</div>"
                        //template: '<span style="float:right">#= kendo.toString(DR_AMT, "n") #</span>'

