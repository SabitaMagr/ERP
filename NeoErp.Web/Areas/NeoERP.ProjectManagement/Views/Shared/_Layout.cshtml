            });




            $("#formFilterText").keyup(function (e) {

                var FilterValue = $(this).val();
                var value = FilterValue.substring(0, 1).toUpperCase() + FilterValue.substring(1);

                if (FilterValue !== "") {
                    var treeview = $('#formTreeView').data("kendoTreeView");
                    treeview.expand(".k-item");
                    $(".selectAll").css("visibility", "hidden");

                    $("#formTreeView .k-group .k-group .k-in").closest("li").hide();
                    $("#formTreeView .k-group").closest("li").hide();
                    $("#formTreeView .k-group .k-group .k-in:contains(" + value + ")").each(function () {
                        $(this).parents("ul, li").each(function () {
                            var treeView = $("#formTreeView").data("kendoTreeView");
                            treeView.expand($(this).parents("li"));
                            $(this).show();
                        });
                    });
                    $("#formTreeView .k-group .k-in:contains(" + FilterValue + ")").each(function () {
                        $(this).parents("ul, li").each(function () {
                            $(this).show();
                        });
                    });
                }
                else {
                    $("#formTreeView .k-group").find("li").show();
                    var nodes = $("#formTreeView > .k-group > li");

                    $.each(nodes, function (i, val) {
                        if (nodes[i].getAttribute("data-expanded") == null) {
                            $(nodes[i]).find("li").hide();
                        }
                    });
                    $(".selectAll").css("visibility", "visible");
                    var treeview = $('#formTreeView').data("kendoTreeView");
                    treeview.collapse(".k-item");
                }
            });
        });

