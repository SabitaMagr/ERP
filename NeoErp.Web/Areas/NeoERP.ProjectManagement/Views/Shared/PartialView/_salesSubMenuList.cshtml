<style>
    .portlet > .portlet-title > .splitterfont {
        font-size: 15px;
    }

 
    .portlet > .portlet-title {
        min-height: 38px;
    }

    .horizontalline {
        margin: 2px;
    }


    ul {
        list-style-type: none;
        margin-left: -47px;
    }
    .k-splitter .k-scrollable {
        overflow: hidden;
    }
    .tab-box .portlet-body {
        overflow: auto;
        max-height: 460px;
    }
</style>
<div class="portlet box green-box tab-box">
    <div class="portlet-title">
        <div class="caption splitterfont">
            <i class="fa fa-gift"></i> 
            <span class="caption-subject bold uppercase">{{modulename}}</span>
</div>
    </div>
    <div class="portlet-body">
        <div class="form-body" id="moduleList">
            <ul>
                @*AA*@
                <li ng-repeat="menu in subMenuList" ng-click="BindDetailGrid(menu.FORM_CODE, menu.TABLE_NAME,menu.FORM_EDESC)">
                    <a>
                        <span class="title" >({{menu.FORM_CODE}}) {{menu.FORM_EDESC}}</span>
                        <span class="selected"></span>
                    </a>
                    <hr class="horizontalline" />
                </li>

            </ul>
        </div>
    </div>
</div>
<script>
    $(document).ready(function () {
        $('[data-toggle="tooltip"]').tooltip();
    });
</script>