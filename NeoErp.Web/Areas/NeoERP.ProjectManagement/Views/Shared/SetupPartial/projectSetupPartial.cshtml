                                <td>Status</td>
                                <td></td>
                            </tr>
                            <tr ng-repeat="project in projectFormList" id="{{project.project.SubProjectId}}">
                                <td>
                                    <input type="text" name="projectSubname_{{project.project.SubProjectId}}" id="projectSubname_{{project.project.SubProjectId}}" ng-model="project.project.SUB_PROJECTNAME" required="required" class="form-control" style="padding:0px 6px;">

                                </td>
                                <td>
                                    <input type="file" name="image_{{project.project.project.SubProjectId}}" id="image_{{project.project.project.SubProjectId}}" ng-model="project.project.IMAGE_NAME" required="required" class="form-control" accept="image/*">
                                </td>
                                <td>
                                    <input type="text" name="area_{{project.project.SubProjectId}}" id="area_{{project.project.SubProjectId}}" ng-model="project.project.AREA" required="required" class="form-control" value="">

                                </td>
                                <td>
                                    <input type="text" ng-click="viewMaterialPlanningModal(project.project.SubProjectId)" style="background-color:aqua;" ng-model="project.project.MATERIAL_PLANNING" class="form-control">
                                </td>
                                <td>
                                    <input type="text" ng-click="viewLabourPlanningModal(project.project.SubProjectId)" style="background-color:darksalmon;" ng-model="project.project.LABOUR_PLANNING" class="form-control">
                                </td>
                                <td>
                                    <input type="text" name="budget_{{project.project.SubProjectId}}" id="budget_{{project.project.SubProjectId}}" ng-model="project.project.BUDGET_PLANNING" required="required" class="form-control" value="">

                                </td>
                                <td>
                                    <select name="projectManager_{{project.project.SubProjectId}}" id="projectManager_{{project.project.SubProjectId}}" ng-model="project.project.PROJECT_MANAGER" required="required" class="form-control"
                                            ng-options="employeeTypeList.EMPLOYEE_EDESC for employeeTypeList in employeeList track by employeeTypeList.EMPLOYEE_CODE">
                                    </select>
                                </td>
                                <td>
                                    <select name="contractor_{{project.project.SubProjectId}}" id="contractor_{{project.project.SubProjectId}}" ng-model="project.project.CONTRACTOR" required="required" class="form-control"
                                            ng-options="contractorTypeList.SUPPLIER_EDESC for contractorTypeList in contractorList track by contractorTypeList.SUPPLIER_CODE">
                                    </select>
                                </td>
                                <td>
                                    <input name="startDt_{{project.project.SubProjectId}}" kendo-date-picker id="startDt_{{project.project.SubProjectId}}" ng-model="project.project.START_DT">

                                </td>
                                <td>
                                    <input name="deadlineDt_{{project.project.SubProjectId}}" kendo-date-picker id="deadlineDt_{{project.project.SubProjectId}}" ng-model="project.project.END_DT">
                                </td>
                                <td>
                                    <select id="status_{{project.subProjectId}}" name="status_{{project.project.SubProjectId}}" ng-model="project.STATUS">
                                        <option value="S">Select</option>
                                        <option value="H">On hold</option>
                                        <option value="R">Running</option>
                                        <option value="C">Completed</option>
                                    </select>
                                </td>
                                <td style="padding:15px;" class="checkboxgap">
                                    <div class="md-checkbox">
                                        <input type="checkbox" id="{{project.SubProjectId}}" class="md-check" ng-model="project.checked">
                                        <label for="{{project.subProjectId}}">
                                            <span class="inc"></span>
                                            <span class="check"></span>
                                            <span class="box"></span>
                                        </label>
                                    </div>
                                </td>
                            </tr>
                            <tr>
                                <td colspan="10"></td>
                                <td style="align-content:center">
                                    <a href="javascript:;" class='btn btn-sm btn-success' ng-click="addProject()">
                                        <i class="fa fa-plus"></i>
                                    </a>
                                </td>
                                <td>
                                    <a href="javascript:;" class='btn btn-sm btn-danger' ng-click="deleteProject()">
                                        <i class="fa fa-trash-o"></i>
                                    </a>
                                </td>
                            </tr>

                        </table>
                        <input type="submit" class="hidden" id="btnProject" value="submit">
                    </form>
                </div>

                <div class="modal-footer">
                    <button type="button" class="btn btn-sm btn-danger" ng-click="ItemCancel(event)" data-dismiss="modal">
                        <i class="fa-solid fa-xmark"></i> Cancel
                    </button>
                </div>
            </div>
        </div>
    </div>
    <div id="editMaterialModal" class="modal fade" role="dialog">
        <div class="modal-dialog">
            <!-- Modal content-->
            <div class="modal-content" style="margin-left: -24%; width: 88rem;">
                <div class="modal-header">
                    <button type="button" class="close" data-dismiss="modal">&times;</button>
                    <h4 class="modal-title">Material Planning</h4>
                </div>
                <div class="modal-body">
                    <form name="materialPlanningForm" ng-submit="submitMaterialPlanning()" id="materialForm" novalidate>
                        <input type="hidden" name="projectId" id="projectId" value="{{selectedProjectForMaterialPlanning.projectId}}">
                        <table class="table table-bordered">
                            <thead>
                                <tr>
                                    <th>Description</th>
                                    <th>Quantity</th>
                                    <th>Rate</th>
                                    <th>Amount</th>
                                </tr>
                            </thead>
                            <tbody>
                                <tr ng-repeat="material in materialPlanningFormList" id="{{material.data.Id}}">
                                    <td>
                                        <input name="itemId_{{material.data.Id}}}" id="itemId_{{material.data.Id}}}" class="form-control" ng-model="material.data.DESCRIPTION" ng-options="itemTypeList.ITEM_EDESC for itemTypeList in itemList track by itemTypeList.ITEM_CODE">
                                    </td>
                                    <td><input type="number" name="quantity_{{material.data.Id}}}" id="quantity_{{material.data.Id}}}" class="form-control" ng-model="material.data.QUANTITY" required></td>
                                    <td><input type="number" name="rate_{{material.data.Id}}}" id="rate_{{material.data.Id}}}" class="form-control" ng-model="material.data.RATE" required></td>
                                    <td><input type="number" name="amount_{{material.data.Id}}}" id="amount_{{material.data.Id}}" class="form-control" ng-model="material.data.AMOUNT" required></td>

                                </tr>
                            </tbody>
                        </table>
                    </form>
                </div>
            </div>
        </div>
    </div>
    <div id="editLabourModal" class="modal fade" role="dialog">
        <div class="modal-dialog">
            <!-- Modal content-->
            <div class="modal-content" style="margin-left: -24%; width: 88rem;">
                <div class="modal-header">
                    <button type="button" class="close" data-dismiss="modal">&times;</button>
                    <h4 class="modal-title">Labour Planning</h4>
                </div>
                <div class="modal-body">
                    <form name="labourPlanningForm" id="labourForm" ng-submit="submitLabourPlanning()" novalidate>
                        <input type="hidden" name="planId" id="planId" value="@ViewBag.Id">
                        <table class="table table-bordered">
                            <thead>
                                <tr>
                                    <th>Description</th>
                                    <th>Quantity</th>
                                    <th>Rate</th>
                                    <th>Amount</th>
                                </tr>
                            </thead>
                            <tbody>
                                <tr ng-repeat="labour in labourPlanningFormList" id="{{labour.data.Id}}">
                                    <td><input type="text" name="description_{{labour.data.Id}}" id="description_{{labour.data.Id}}" class="form-control" ng-model="labour.data.DESCRIPTION" required></td>
                                    <td><input type="number" name="quantity_{{labour.data.Id}}" id="quantity_{{labour.data.Id}}" class="form-control" ng-model="labour.data.QUANTITY" required></td>
                                    <td><input type="number" name="rate_{{labour.data.Id}}" id="rate_{{labour.data.Id}}" class="form-control" ng-model="labour.data.RATE" required></td>
                                    <td><input type="number" name="amount_{{labour.data.Id}}" id="amount_{{labour.data.Id}}" class="form-control" ng-model="labour.data.AMOUNT" required></td>
                                </tr>
                            </tbody>
                        </table>
                    </form>
                </div>
            </div>
        </div>
    </div>
</div>
