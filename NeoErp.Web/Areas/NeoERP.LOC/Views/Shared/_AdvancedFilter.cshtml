<script>
    $(document).ready(function () {

        $("#filterTreeTabs").hide();

        function activateTab(tab) {
            $('.nav-tabs a[href="#' + tab + '"]').tab('show');
        };
        $("#customerToggle").click(function () {
            $(".customerSlidebox").slideToggle();
            activateTab('customerTab');
            $("#customerTreeView").data("kendoTreeView").dataSource.bind("change",
            $("#customerTreeView").data("kendoTreeView").dataSource.read()
            );
            toggleIcon($(this));
        });
        $("#supplierToggle").click(function () {
            $(".supplierSlidebox").slideToggle();
            activateTab('supplierTab');
            $("#supplierTreeView").data("kendoTreeView").dataSource.bind("change",
            $("#supplierTreeView").data("kendoTreeView").dataSource.read()
            );
            toggleIcon($(this));
        });
        // -- elixir
        $("#accountToggle").click(function () {
            $(".accountSlidebox").slideToggle();
            activateTab('accountTab');
            $("#accountTreeView").data("kendoTreeView").dataSource.bind("change",
            $("#accountTreeView").data("kendoTreeView").dataSource.read()
            );
            toggleIcon($(this));
        });
        $("#locationToggle").click(function () {
            
            $(".locationSlidebox").slideToggle();
            activateTab('locationTab');
            $("#locationTreeView").data("kendoTreeView").dataSource.bind("change",
            $("#locationTreeView").data("kendoTreeView").dataSource.read()
            );
            toggleIcon($(this));
        });

        $("#productToggle").click(function () {
            $(".productSlidebox").slideToggle();
            activateTab('productTab');
            $("#productTreeView").data("kendoTreeView").dataSource.bind("change",
                $("#productTreeView").data("kendoTreeView").dataSource.read()
