            toggleIcon($(this));
        });


        $("#supplierFilterButton").click(function () {
            $("#filterTreeTabs").show();
            activateTab('supplierTab');
            $("#supplierTreeView").data("kendoTreeView").dataSource.bind("change",
                $("#supplierTreeView").data("kendoTreeView").dataSource.read()
            );
            toggleIcon($(this));
        });

        $("#documentFilterButton").click(function () {
            $("#filterTreeTabs").show();
            activateTab('documentTab');
            $("#documentTreeView").data("kendoTreeView").dataSource.bind("change",
                $("#documentTreeView").data("kendoTreeView").dataSource.read()
            );
            toggleIcon($(this));
        });

        $("#purchaseDocumentFilterButton").click(function () {
            $("#filterTreeTabs").show();
            activateTab('purchaseDocumentTab');
            $("#purchaseDocumentTreeView").data("kendoTreeView").dataSource.bind("change",
                $("#purchaseDocumentTreeView").data("kendoTreeView").dataSource.read()
            );
            toggleIcon($(this));
        });

