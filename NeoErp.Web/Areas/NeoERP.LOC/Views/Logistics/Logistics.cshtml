    <script type="text/javascript">

        $("#check").click(function () {
            alert("yes");
        });
        $('.nav-tabs a').on('click', function (e) {

            e.preventDefault();

        });

        var count = 1;
        myDropzone = new Dropzone('#myAwesomeDropzone', { // Make the whole body a dropzone
            thumbnailWidth: 50,
            thumbnailHeight: 50,
            autoDiscover: false,
            autoProcessQueue: false,
            addRemoveLinks: true,
            paramName: "inputFiles",
            uploadMultiple: true,
            parallelUploads: 100,
            maxFiles: 100,
            maxFileSize: 10,
        });

        myDropzone.on("success", function (file, responseText) {

           

        });
        myDropzone.on("addedfile", function (file, responseText, option) {

            if (responseText != "edit") {
                var lengths = myDropzone.files.length;
                file["lengths"] = lengths;
                angular.element('#LogisticsCtrl').scope().addIssue();
                $(".dz-message").css({ "display": "none" });
            }
        });
        myDropzone.on("removedfile", function (file) {

            var totallengths = myDropzone.files.length;
            file["lengths"] = totallengths;
            angular.element('#LogisticsCtrl').scope().RemoveIssue(file.lengths, totallengths);
            var $this = $(this);
            if (file.hasOwnProperty("LC_TRACK_NO")) {
                var imageremovedetails = {
                    SNO: file.SNO,
                    LC_TRACK_NO: file.LC_TRACK_NO,
                    INVOICE_NO: file.INVOICENO,
                };

                $.ajax({
                    url: "/api/Logistics/RemoveLogisticImage",
                    type: "POST",
                    dataType: "json",
                    data: imageremovedetails,
                    success: function (result) {
                        RefreshGrid();

                        displayPopupNotification("Succesfully Removed File.", "Success");
                    },
                    error: function (result) {

                    }
                });
            }
        });
        myDropzone.on("complete", function (file) {

            myDropzone.removeFile(file);
        });
        $(document).on('click', '.maskdate', function () {
            var me = $(this);

            me.datepicker({
                showOn: 'focus',
                altFormat: "mm/dd/yy",
                dateFormat: "mm/dd/yy",
                changeMonth: true,
                changeYear: true,
                showButtonPanel: true

            }).focus();
        }).on('focus', '.maskdate', function () {
            var me = $(this);
            me.mask('99/99/9999');
        }).on('change', '.maskdate', function () {
            var date = $(this).val();
            var arr = date.split('/');
            var month = (arr[0]);
            var day = (arr[1]);
            if (month > 12 || day > 32) {

                $(this).val(new Date('mm/dd/yy'));
            }


        });
        function RefreshGrid() {
            $("#grid").data("kendoGrid").dataSource.read();
        }

    </script>
