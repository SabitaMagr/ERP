</style>
<div ng-app="myApp">
    <div ng-controller="LogisticsCtrl" id="LogisticsCtrl">


        <div class="portlet light bordered main">
            <div class="portlet-title titleheading">
                <i class="fa fa-list-alt fontgreen"></i>
                <span class="caption-subject fontgreen bold uppercase">
                    Logistics Entry
                </span>
                <span class="pull-right" ng-show="!showFormPanel">
                   <button type="button" id="addTarget" name="addTarget" ng-click="AddLogistics()" class="btn buttongreen btn-sm">[+] Add</button>
                </span>

                <span class="pull-right" ng-show="showFormPanel">
                    <button type="submit" id="EditContent" name="EditContent" ng-hide="ContentModal" ng-click="EditContent()" class="btn btn-default btn-sm" data-toggle="modal" data-target="#ContentModal"> Edit Content</button>
                    <button type="button" id="cancelsi" name="cancelsi" class="btn btn-default btn-sm" ng-click="cancelsi();">Cancel</button>
                    <button type="submit" id="saveshipment" name="saveshipment" ng-click="createshipment(ShipmentInvoiceForm.$valid,'save')" class="btn buttongreen btn-sm">{{saveAction}}</button>
                </span>

            </div><!-- /portlet-title-->
            <div class="portlet-body">
                <div class="row">

                    <form id="ShipmentInvoiceForm" name="ShipmentInvoiceForm" ng-show="showFormPanel" novalidate>

                        <input type="hidden" id="LocCode" ng-model="pi.LocCode" />
                        <div ng-hide="oneditshow">
                            <div class="col-sm-6 col-md-3">
                                <div class="form-group">
                                    <label class="control-label">
                                        Invoice Number<span style="color:red" ng-show="ShipmentInvoiceForm.pininvoice.$invalid">
                                            <span ng-show="ShipmentInvoiceForm.pininvoice.$error.required">*</span>
                                        </span>
                                    </label>
                                    <input id="perfomainvoiceautocomplete" ng-model="si.perfomainvoice" name="perfomainvoice" kendo-auto-complete
                                           k-data-text-field="'INVOICE_NUMBER'"
                                           k-data-value-field="'INVOICE_CODE'"
                                           k-data-source="ipPurchaseOrder"
                                           k-min-length="1"
                                           k-value-primitive="true"
                                           k-on-select="ItemsOnChange(kendoEvent)"
                                           k-placeholder="'Select Commercial Number...'"
                                           style="width: 100%" ng-disabled="profomainvoicedisabled" />

                                </div>
                            </div>

                            <div ng-show="HideShippment">
                                <div class="col-sm-12 col-md-12">
                                    <fieldset>
                                        <legend>Commercial Item</legend>
                                     
                                        <div class="portlet-body">
                                       
                                            <div class="tblresponsive">
                                                <table class="table table-striped  table-hover ctable" id="items">
                                                    <thead>
                                                        <tr>
                                                            <th width="5%">S.N.</th>
                                                            <th>Invoice Date</th>
                                                            <th>Item</th>
                                                            <th>Qty</th>
                                                            <th>Amount</th>
                                                            <th>Total</th>

                                                    <tbody>
                                                        <tr style="font-size:10px;" ng-if="itemlist.length==0"><td colspan="11">No Items To Display..</td></tr>
                                                        <tr style="font-size:10px;" ng-if="itemlist.length!=0" ng-repeat="items in itemlist">
                                                            <td>{{$index +1}}</td>
                                                            <td>{{items.INVOICE_DATE | date:'MM/dd/yyyy'}}</td>
                                                            <td>{{items.ITEM_EDESC}}</td>
                                                            <td style="text-align:right;">{{items.QUANTITY | number : fractionSize}}</td>
                                                            <td style="text-align:right;">{{items.AMOUNT | currency :"": fractionSize}}</td>
                                                            <td style="text-align:right;">{{ (items.QUANTITY) * (items.AMOUNT) |currency:"":fractionSize}}</td>

                                                        </tr>
                                                        <tr>
                                                            <td></td>
                                                            <td><b>Grand Total</b></td>
                                                            <td></td>
                                                            <td><b class="pull-right">{{Total_Quantity | number}}</b>  </td>
                                                            <td>@*<b class="pull-right">{{Rate | number}}</b>*@ </td>
                                                            <td><b class="pull-right">{{Total_Amount | number}} </b></td>
                                                     
                                                        </tr>
                                                    </tbody>
                                                </table>
                                            </div>
                                        </div>
                                    </fieldset>
                                </div>

                                <div class="col-sm-12 col-md-12">
                                    <fieldset>
                                        <legend>Shipment Details</legend>
                                        <div class="row">
                                            <input type="hidden" ng-model="lctrackno" />


                                            <div class="col-sm-12 col-md-3">
                                                <div class="form-group">
                                                    <label class="control-label">
                                                        Shipment Type
                                                    </label>
                                                    <select id="shipmentType" k-value-primitive="true" k-on-change="OnChangeShipmentType(kendoEvent)" name="shipmentType" k-ng-model="si.shipmentType" ng-model="si.shiptype" kendo-drop-down-list style="width: 100%"
                                                            k-options="shipmentType" ng-disabled="true"></select>
                                                </div>
                                            </div>

                                            <div class="col-sm-6 col-md-3" ng-show="commercialnumber">
                                                <div class="form-group">
                                                    <label class="control-label">
                                                        Commercial Invoice Number
                                                    </label>
                                                    <input type="text" value="{{invoiceno}}" class="form-control input-sm" disabled />
                                                </div>
                                            </div>


                                            <!------------------------------------------For Air-------------------------------------->
                                            <div ng-show="showforair">
                                                <div class="col-md-12">
                                                    <h5><b><u>Air</u></b></h5>
                                                </div>
                                                <div class="col-sm-6 col-md-3">
                                                    <div class="form-group">
                                                        <label class="control-label">
                                                            Airway Bill No.
                                                        </label>
                                                        <input type="text" id="awb" name="awb" ng-model="si.awb" class="form-control input-sm" ng-disabled="!si_check_awb" />

                                                    </div>
                                                </div>
                                                <div class="col-sm-6 col-md-3">
                                                    <div class="form-group">
                                                        <label class="control-label">Airway Bill Date</label>
                                                        <input type="text" id="awbdate" name="awbdate" autocomplete="off" class="form-control maskdate" ng-model="si.awbdate" ng-disabled="!si_check_awbdate" />

                                                    </div>
                                                </div>
                                                <div class="col-sm-6 col-md-3">
                                                    <div class="form-group">
                                                        <label class="control-label">
                                                            Gross Weight Air
                                                        </label>
                                                        <input type="number" id="grosswhtair" name="grosswhtair" ng-model="si.grosswhtair" class="form-control input-sm" ng-disabled="!si_check_grosswhtair" />
                                                    </div>
                                                </div>

                                                <div class="col-sm-6 col-md-3">
                                                    <div class="form-group">
                                                        <label class="control-label">
                                                            Chargeable Weight Air
                                                        </label>
                                                        <input type="number" id="chargeablewhtair" name="chargeablewhtair" ng-model="si.chargeablewhtair" class="form-control input-sm" ng-disabled="!si_check_chargeablewhtair" />
                                                    </div>

                                                </div>

                                                <div class="col-sm-6 col-md-3">
                                                    <div class="form-group">
                                                        <label class="control-label">
                                                          No. of Palettes
                                                        </label>
                                                        <input type="number" id="AIR_PACK" name="AIR_PACK" ng-model="si.airpack" class="form-control input-sm" ng-disabled="!si_check_airpack" />
                                                    </div>

                                                </div>



                                            </div>
                                            <!------------------------------------------For Sea-------------------------------------->
                                            <div ng-show="showforsea">
                                                <div class="col-md-12">
                                                    <h5><b><u>Sea</u></b></h5>
                                                </div>
                                                <div class="col-sm-6 col-md-3">
                                                    <div class="form-group">
                                                        <label class="control-label">
                                                            Lading Bill No.
                                                        </label>
                                                        <input type="text" id="lading" name="lading" ng-model="si.lading" class="form-control input-sm" ng-disabled="!si_check_lading" />

                                                    </div>
                                                </div>
                                                <div class="col-sm-6 col-md-3">
                                                    <div class="form-group">
                                                        <label class="control-label">Lading Bill Date</label>
                                                        <input type="text" id="ladingdate" name="ladingdate" autocomplete="off" class="form-control maskdate" ng-model="si.ladingdate" ng-disabled="!si_check_ladingdate" />

                                                    </div>
                                                </div>
                                                <div class="col-sm-6 col-md-3">
                                                    <div class="form-group">
                                                        <label class="control-label">
                                                            Gross Weight Sea
                                                        </label>
                                                        <input type="number" id="grossweightsea" name="grossweightsea" ng-model="si.grossweightsea" class="form-control input-sm" ng-disabled="!si_check_grossweightsea" />

                                                    </div>
                                                </div>

                                                <div class="col-sm-6 col-md-3">
                                                    <div class="form-group">
                                                        <label class="control-label">
                                                            Measurement weight (CBM for sea)
                                                        </label>
                                                        <input type="number" id="cbmsea" name="cbmsea" ng-model="si.cbmsea" class="form-control input-sm" ng-disabled="!si_check_cbmsea" />
                                                    </div>
                                                </div>
                                                <div class="col-sm-6 col-md-3">
                                                    <div class="form-group">
                                                        <label class="control-label">
                                                            Vessel No
                                                        </label>
                                                        <input type="text" id="vesselno" name="vesselno" ng-model="si.vesselno" class="form-control input-sm" ng-disabled="!si_check_vesselno" />
                                                    </div>
                                                </div>

                                            </div>
                                            <!------------------------------------------For Road------------------------------------->
                                            <div ng-show="showforroad">
                                                <div class="col-md-12">
                                                    <h5><b><u>Road</u></b></h5>
                                                </div>
                                                <div class="col-sm-6 col-md-3">
                                                    <div class="form-group">
                                                        <label class="control-label">
                                                            Road Bill No.
                                                        </label>
                                                        <input type="text" id="road" name="road" ng-model="si.road" class="form-control input-sm" ng-disabled="!si_check_road" />

                                                    </div>
                                                </div>
                                                <div class="col-sm-6 col-md-3">
                                                    <div class="form-group">
                                                        <label class="control-label">Road Bill Date</label>
                                                        <input type="text" id="roaddate" name="roaddate" autocomplete="off" class="form-control maskdate" ng-model="si.roaddate" ng-disabled="!si_check_roaddate" />

                                                    </div>
                                                </div>
                                                <div class="col-sm-6 col-md-3">
                                                    <div class="form-group">
                                                        <label class="control-label">
                                                            Truck No
                                                        </label>
                                                        <input type="text" id="truck" name="truck" ng-model="si.truck" class="form-control input-sm" ng-disabled="!si_check_truck" />
                                                    </div>
                                                </div>
                                                <div class="col-sm-6 col-md-3">
                                                    <div class="form-group">
                                                        <label class="control-label">
                                                            Transporter Name
                                                        </label>
                                                        <input type="text" id="transname" name="transname" ng-model="si.transname" class="form-control input-sm" ng-disabled="!si_check_transname" />

                                                    </div>
                                                </div>
                                                <div class="col-sm-6 col-md-3">
                                                    <div class="form-group">
                                                        <label class="control-label">
                                                            Transporter Address
                                                        </label>
                                                        <input type="text" id="transaddress" name="transaddress" ng-model="si.transaddress" class="form-control input-sm" ng-disabled="!si_check_transaddress" />

                                                    </div>
                                                </div>

                                            </div>
                                            <!------------------------------------------For Train------------------------------------>
                                            <div ng-show="showfortrain">
                                                <div class="col-md-12">
                                                    <h5><b><u>Train</u></b></h5>
                                                </div>
                                                <div class="col-sm-6 col-md-3">
                                                    <div class="form-group">
                                                        <label class="control-label">
                                                            Train Bill No.
                                                        </label>
                                                        <input type="text" id="train" name="train" ng-model="si.train" class="form-control input-sm" ng-disabled="!si_check_train" />

                                                    </div>
                                                </div>
                                                <div class="col-sm-6 col-md-3">
                                                    <div class="form-group">
                                                        <label class="control-label">Train Bill Date</label>
                                                        <input type="text" id="traindate" name="traindate" autocomplete="off" class="form-control maskdate" ng-model="si.traindate" ng-disabled="!si_check_traindate" />

                                                    </div>
                                                </div>

                                            </div>
                                            <!------------------------------------------For Common------------------------------------>
                                            <div class="row" style="margin:0px; padding:0px;" ng-show="showforcommon">
                                                @*SUBIN FROM*@
                                                <!--CommonColumnsStart For Air-->


                                                <div class="col-sm-6 col-md-3">
                                                    <div class="form-group">
                                                        <label class="control-label">
                                                            Contractor Name
                                                        </label>
                                                        <select id="contractorName" k-value-primitive="true" name="contractorName" k-data-text-field="'CONTRACTOR_EDESC'" k-data-value-field="'CONTRACTOR_CODE'" k-ng-model="contractorCode" ng-model="contractorCode" kendo-drop-down-list style="width: 100%"
                                                                k-data-source="contractorNameDatasource" k-option-label="'--Select  Contractor Name--'" ng-disabled="!si_check_contractorCode"></select>
                                                    </div>
                                                </div>
                                                <div class="col-sm-6 col-md-3">
                                                    <div class="form-group">
                                                        <label class="control-label">
                                                            Contractor Address
                                                        </label>
                                                        <input type="text" name="contractoraddress" ng-model="contractorAddress" class="form-control input-sm" ng-disabled="!si_check_contractorAddress" />

                                                    </div>
                                                </div>
                                                <div class="col-sm-6 col-md-3">
                                                    <div class="form-group">
                                                        <label class="control-label">
                                                            Contract Amount
                                                        </label>
                                                        <input type="number" name="contractAmt" ng-model="contractAmt" class="form-control input-sm" ng-disabled="!si_check_contractAmt" />

                                                    </div>
                                                </div>
                                                <div class="col-sm-6 col-md-3">
                                                    <div class="form-group">
                                                        <label class="control-label">Contract Date </label>
                                                        <input type="text" name="contractDate" autocomplete="off" class="form-control maskdate" ng-model="contractDate" ng-disabled="!si_check_contractDate" />

                                                    </div>
                                                </div>
                                                <div class="col-sm-6 col-md-3">
                                                    <div class="form-group">
                                                        <label class="control-label">
                                                            Job Order No
                                                        </label>
                                                        <input type="text" name="jobOrderNo" ng-model="jobOrderNo" class="form-control input-sm" ng-disabled="!si_check_jobOrderNo" />

                                                    </div>
                                                </div>
                                                <div class="col-sm-6 col-md-3">
                                                    <div class="form-group">
                                                        <label class="control-label">
                                                            Shipper Name
                                                        </label>
                                                        <input type="text" name="shipperName" ng-model="shipperName" class="form-control input-sm" ng-disabled="!si_check_shipperName" />
                                                    </div>

                                                </div>
                                                <div class="col-sm-6 col-md-3">
                                                    <div class="form-group">
                                                        <label class="control-label">
                                                            Shipper Address
                                                        </label>
                                                        <input type="text" name="shipperAddress" ng-model="shipperAddress" class="form-control input-sm" ng-disabled="!si_check_shipperAddress" />
                                                    </div>

                                                </div>

                                             
                                                <div class="col-sm-6 col-md-3">
                                                    <div class="form-group">
                                                        <label class="control-label">
                                                            Issuing Carrier
                                                        </label>
                                                        <select id="issuingcarrer" k-value-primitive="true" name="issuingcarrer" k-data-text-field="'CARRIER_EDESC'" k-data-value-field="'CARRIER_CODE'" k-ng-model="si.issuingcarrer" ng-model="si.issuingcarrer" kendo-drop-down-list style="width: 100%"
                                                                k-data-source="issuingCarrierDatasource" k-option-label="'--Select Issuing Carrier--'" ng-disabled="!si_check_issuingcarrer"></select>

                                                    </div>
                                                </div>


                                                <div class="col-sm-6 col-md-3">
                                                    <div class="form-group">
                                                        <label class="control-label">
                                                            Clearing Agent
                                                        </label>
                                                        <select id="clearingAgent" k-value-primitive="true" name="clearingAgent" k-data-text-field="'CAGENT_EDESC'" k-data-value-field="'CAGENT_CODE'" k-ng-model="clearingAgent" ng-model="clearingAgent" kendo-drop-down-list style="width: 100%"
                                                                k-data-source="clearingAgentDatasource" k-option-label="'--Select Clearing Agent--'" ng-disabled="!si_check_clearingAgent"></select>

                                                    </div>
                                                </div>

                                                <div class="col-sm-6 col-md-3">
                                                    <div class="form-group">
                                                        <label class="control-label">
                                                            Remarks
                                                        </label>
                                                        <input type="text" id="remarks" name="remarks" ng-model="si.remarks" class="form-control input-sm" ng-disabled="!si_check_remarks" />
                                                    </div>
                                                </div>


                                         
                                            </div>
                                            <div ng-show="showforcommon">

                                                <div class="source">
                                                    <h5><b> Source</b></h5>
                                                    <div class="row" style="margin:0px; padding:0px;">
                                                        <div class="col-sm-6 col-md-3">
                                                            <div class="form-group">
                                                                <label class="control-label">
                                                                    From Location<span style="color:red" ng-show="ShipmentInvoiceForm.fromLocation.$invalid">
                                                                        <span ng-show="ShipmentInvoiceForm.fromLocation.$error.required">*</span>
                                                                    </span>
                                                                </label>
                                                                <select id="fromlocation" k-value-primitive="true" name="fromLocation" k-data-text-field="'LOCATION_EDESC'" k-data-value-field="'LOCATION_CODE'" ng-model="fromLocation" kendo-drop-down-list style="width: 100%"
                                                                        k-data-source="locationDatasource" k-option-label="'--Select Location--'" ng-change="LoadETA()" required ng-disabled="!si_check_fromLocation"></select>

                                                            </div>
                                                        </div>
                                                    </div>
                                                    <div class="row" style="margin:0px; padding:0px;" ng-show="ShowContainer">
                                                        <div class="col-sm-4 col-md-4">
                                                            <div class="form-group">
                                                                <label class="control-label">Estimated Time of Arrival (ETA)</label>
                                                                <input type="text" name="etaDate" autocomplete="off" class="form-control maskdate" ng-model="SRC_ETA" ng-disabled="!si_check_SRC_ETA" />

                                                            </div>
                                                        </div>
                                                        <div class="col-sm-4 col-md-4">
                                                            <div class="form-group">
                                                                <label class="control-label">Actual Time of Arrival (ATA)</label>
                                                                <input type="text" name="ataDate" autocomplete="off" class="form-control maskdate" ng-model="SRC_ATA" ng-disabled="!si_check_SRC_ATA" />
                                                            </div>
                                                        </div>
                                                        <div class="col-sm-4 col-md-4">
                                                            <div class="form-group">
                                                                <label class="control-label">Estimated Time of Delivery (EDD)</label>
                                                                <input type="text" name="addDate" autocomplete="off" class="form-control maskdate" ng-model="SRC_ETD_DES" ng-disabled="!si_check_SRC_ETD_DES" />

                                                            </div>
                                                        </div>
                                                        <div class="col-sm-4 col-md-4">
                                                            <div class="form-group">
                                                                <label class="control-label">Estimated Time of Departure (ETD)</label>
                                                                <input type="text" name="addDate" autocomplete="off" class="form-control maskdate" ng-model="SRC_ETD" ng-disabled="!si_check_SRC_ETD" />
                                                            </div>
                                                        </div>
                                                        <div class="col-sm-4 col-md-4">
                                                            <div class="form-group">
                                                                <label class="control-label">Actual Time of Departure (ATD)</label>
                                                                <input type="text" name="etdDate" autocomplete="off" class="form-control maskdate" ng-model="SRC_ATD" ng-disabled="!si_check_SRC_ATD" />

                                                            </div>
                                                        </div>


                                                    </div>
                                                </div>

                                                <div class="destination">
