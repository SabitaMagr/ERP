        function salesplanajaxCall() {
            var url = window.location.protocol + "//" + window.location.host + "/api/PlanSetupApi/CreateTempSalesPlanReportTable";
            var param = ReportFilter.filterAdditionalData();
            $.ajax({
                url: url,
                type: "POST",
                data: param,
                dataType: 'html',
                async: false,
                contentType: "application/json; charset=utf-8",
                beforeSend: function (xhr, opts) {
                    
                    $('#salesplanSubmit').button('loading');
                    $(".portlet-title .btn").attr("disabled", "disabled");
                },
                success: function (data) {
                    
                    if (JSON.parse(data).MESSAGE == "DELETECREATED") {
                        displayPopupNotification("Table Deleted And Created Successfully", "Success");
                        hideloader();
                    }
                    else if (JSON.parse(data).MESSAGE == "CREATED") {
                        displayPopupNotification("Table Created Successfully", "Success");
                        hideloader();
                    }
                    else {
                        displayPopupNotification("There is an error while saving data", "error");
                        hideloader();
                    }
                    $('#salesplanSubmit').button('reset');
                    $(".portlet-title .btn").attr("disabled", false);
                },
                error: function (jqxhr) {
                    
                    displayPopupNotification("There is an Server Error", "error");
                    hideloader();
                    $('#salesplanSubmit').button('reset');
                    $(".portlet-title .btn").attr("disabled", false);
                }
            });
        }
        $('#salesplanSubmit').on("click", function (e) {
            
            $(this).button('loading');
            setTimeout(function () {
                salesplanajaxCall();
            }, 100);
        });
