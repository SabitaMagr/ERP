<style>
    .dashboard_count .title_count:before {
        content: "";
        position: absolute;
        left: 0;
        height: 65px;
        border-left: 2px solid #dbe0e2;
        margin-top: 10px;
    }

    .dashboard_count .title_count:first-child:before {
        border-left: 0;
    }

    .dashboard_count .title_count .count {
        font-size: 42px;
        line-height: 47px;
        font-weight: 700;
        color: #73879C;
    }

    .title_count .top {
        font-size: 13px;
        color: #73879C;
        font-weight: 600 !important;
    }

    .dashboard_count .tile_count {
        border-bottom: 1px solid #D9DEE4;
        padding: 0 10px 0 20px;
        position: relative;
        margin-bottom: 20px;
    }

    .dashboard_count .title_count-first-child:before {
        content: "";
    }

    .dashboard_count .tile_count {
        margin-bottom: 10px;
        color: #73879C;
    }

    .text-green {
        color: rgb(67, 163, 46);
    }

    .title_count {
        padding-bottom: 15px !important;
    }
</style>

<style>
    hr.style-three {
        border: 0;
        border-bottom: 1px dashed #ccc;
        background: #999;
    }

    #gauge-container {
        width: 404px;
        height: 404px;
        text-align: center;
        margin: 0 auto 30px auto;
    }

    #gauge {
        display: inline-block;
        width: 230px;
        height: 230px;
        margin: 0 auto;
    }

    #linearGauge-container {
        display: inline-block;
        text-align: center;
        height: 330px;
        width: 330px;
        background: transparent url("../content/linear-gauge-container.png") no-repeat 50% 50%;
    }

        #linearGauge-container.horizontal {
            background-image: url("../content/linear-gauge-container-h.png");
        }

    #linearGauge {
        width: 310px;
        height: 310px;
        margin-top: 5%;
    }
</style>

