<div class="modal fade" id="highestQueryModel" tabindex="-1" role="dialog" aria-labelledby="filterModalLabel">
    <div class="modal-dialog">
        <div class="modal-content">
            <div class="modal-header">

                <h4 class="modal-title" id="filterModalLabel">Get Highest Value From Query</h4>
            </div>
            <div class="modal-body">

                <div class="">
                    <label class="">
                        Sql Query For Highest Value
                    </label>
                    <div class="">
                        <textarea data-bind="value: sqlHigestQuery" type="text" class="form-control"></textarea>

                    </div>
                </div>
            </div>
            <div class="modal-footer">
                <button type="button" class="btn green" data-bind="click: getHigestLowestValue()" data-dismiss="modal">Apply</button>
                <button type="button" class="btn dark btn-outline" data-dismiss="modal">Close</button>
            </div>
        </div>
    </div>
</div>

<div class="modal fade" id="lowestQueryModel" tabindex="-1" role="dialog" aria-labelledby="filterModalLabel">
