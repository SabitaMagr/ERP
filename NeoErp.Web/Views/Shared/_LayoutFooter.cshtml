<script>

    function SetGridHeight() {
        var div_print = $("#div_print").length;
        if (div_print == 1) {
            var profile = $("#profileheader").height();
            profile = profile != null ? profile + 15 : 0;
            profile = profile + -$('.header-cont').height();
            //$('.scroll').height($(window).height() - $('.header-cont').height() - profile - 190);

            var tab = $(".nav-tabs-custom").length;
            if (tab >= 1) {
                profile = profile + 17;
            }
            profile = profile + 218;
            $('.scroll').height($(window).height()-profile);
        }
    }
    SetGridHeight();
    AddThead();

</script>
