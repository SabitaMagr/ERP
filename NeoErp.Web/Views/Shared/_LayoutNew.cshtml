            //$(document).shortkeys({

            //    'Space+V': function () { $('.setting').show(); },
            //    'Space+h': function () { $('.setting').hide(); },

            //});
            key('ctrl+h', function () { $('.setting').hide(); return false });
            key('ctrl+v', function () { $('.setting').show(); return false });
            $.contextMenu({
                selector: '.bordericon',
                callback: function (key, options) {
                    var path = $(this).attr("href");
                    var urlpath = $(this).find('a:first').attr("href");
                    //  alert(urlpath);
                    $.ajax({
                        type: "POST",
                        url: window.location.protocol + "//" + window.location.host + "/api/Common/SaveDefaultDashboard?Path=" + urlpath,
                        success: function (result) {
                            if (result.Success == "true") {
                                displayBarNotification(result.Message);
                            }
                            displayBarNotification(result.Message, "error");
                        }
                    });

                },
                items: {
                    "Default Dashboard": { name: "Default Dashboard", icon: "Default Dashboard" },

                }
            });
        });
